                      <section
                        class="content-recommendations-component post-bottom"
                        data-component-type="content-recommendations"
                        data-provider="piano"
                        data-widget-id="56f9526ddbbc503aa744df3b2f2cf5e0c2f1a3f4"
                        data-renderer="related-posts"
                        data-container-name="content-recommendations"
                        data-recommendations-placement=""
                      >
                        <div class="content-recommendations-title-container">
                          <svg
                            class="lightning-bolt-container"
                            width="11"
                            height="21"
                            viewBox="0 0 11 21"
                            fill="none" xmlns="http://www.w3.org/2000/svg"
                          >
                            <title>Deal icon</title>
