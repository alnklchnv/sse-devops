            eventCategory: 'slide prompt',
            eventAction: 'click - no thanks',
            event: 'engagement'
          });
        });
  
        // Send tag with the id if the user is on a vertical page
  
        // checking cookie if user is a bi prime user, send the 'prime' tag to OneSignal
        if (document.cookie.split(';').filter(function (item) {return item.indexOf('token_type=piano') > -1 }).length) {
          OneSignal.sendTag('prime', true);
        }
      });
      </script>
