  }

  /**
   * 1. Add the correct box sizing in IE 10.
   * 2. Remove the padding in IE 10.
   */

  [type="checkbox"],
  [type="radio"] {
    box-sizing: border-box; /* 1 */
    padding: 0; /* 2 */
  }

  /**
   * Correct the cursor style of increment and decrement buttons in Chrome.
   */

  [type="number"]::-webkit-inner-spin-button,
  [type="number"]::-webkit-outer-spin-button {
    height: auto;
  }

  /**
   * 1. Correct the odd appearance in Chrome and Safari.
   * 2. Correct the outline style in Safari.
   */

  [type="search"] {
    -webkit-appearance: textfield; /* 1 */
    outline-offset: -2px; /* 2 */
  }

  /**
   * Remove the inner padding in Chrome and Safari on macOS.
   */

  [type="search"]::-webkit-search-decoration {
    -webkit-appearance: none;
  }

  /**
   * 1. Correct the inability to style clickable types in iOS and Safari.
   * 2. Change font properties to 'inherit' in Safari.
   */

  ::-webkit-file-upload-button {
    -webkit-appearance: button; /* 1 */
    font: inherit; /* 2 */
  }

  /* Interactive
    ========================================================================== */

  /*
   * Add the correct display in Edge, IE 10+, and Firefox.
   */

  details {
    display: block;
  }

  /*
   * Add the correct display in all browsers.
   */

  summary {
    display: list-item;
  }

  /* Misc
    ========================================================================== */

  /**
   * Add the correct display in IE 10+.
   */

  template {
    display: none;
  }

  /**
   * Add the correct display in IE 10.
   */

  [hidden] {
    display: none;
  }
  /* stylelint-enable */

  /**
   * As well as using Normalize.css, it is often advantageous to remove all
   * margins from certain elements.
   */
  body,
  h1, h2, h3, h4, h5, h6,
  p, blockquote, pre,
  dl, dd, ol, ul,
  form, fieldset, legend,
  figure,
  table, th, td, caption,
  hr {
    margin: 0;
    padding: 0;
  }

  /**
   * Remove trailing margins from nested lists.
   */
  li > ul,
  li > ol {
    margin-bottom: 0;
  }

  /**
   * Let’s get <table>s behaving how we expect.
   */
  table {
    border-collapse: collapse;
    border-spacing: 0;
  }

  /**
   * Set default spacing to 'margin-bottom' for all block level elements for
   * consistent vertical rhythm.
   * http://csswizardry.com/2012/06/single-direction-margin-declarations/
   */
  h1, h2, h3, h4, h5, h6, hgroup,
  ul, ol, dl,
  blockquote, p, address,
  table,
  fieldset, figure,
  pre {
    margin-bottom: 1rem;
  }

  /**
   * Indent list elements by the same amount.
   */
  ul, ol,
  dd {
    margin-left: 1rem;
  }

  /**
   * Handle Video Styling at Mobile
   */
  @media (max-width: 600px) {
    .article__body iframe[src*="brightcove"],
    .article__body iframe[src*="mtvnservices"],
    .article__body iframe[src*="rumble"],
    .article__body iframe[src*="vimeo"],
    .article__body iframe[src*="youtube"] {
      width: auto;
      height: auto;
      margin-left: auto;
      max-width: 100%;
      margin-right: auto;
      display: block;
    }
  }

  .interactive-youtube {
    padding: 0 5px;
    position: relative;
    padding-bottom: 56.25%;
    height: 0;
  }

  /* stylelint-disable selector-type-no-unknown */
  .article__body iframe[src*="brightcove"], .article-main iframe[src*="brightcove"],
  .article__body amp-iframe[src*="brightcove"], .article-main amp-iframe[src*="brightcove"],
  .article__body amp-iframe[src*="jw-iframe"], .article-main amp-iframe[src*="jw-iframe"],
  .article-main iframe[src*="youtube.com"] {
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 100%;
  }
  /* stylelint-enable selector-type-no-unknown */


    /**
   * Semantically, we’d like all forms to be built with <fieldset>s and
   * <legend>s, but we don’t want to visually see them.
   */
  fieldset {
    border: none;
  }

  /**
   * Make sure our form elements don’t use any User-Agent specific font styles
   */
  button,
  input,
  optgroup,
  select,
  textarea {
    font: inherit;
  }

    /**
   * 1. Fluid images.
   * 2. Differentiate 'alt' text.
   * 3. Removes the whitespace under 'img' elements.
   */
  img {
    max-width: 100%; /* [1] */
    font-style: italic; /* [2] */
    vertical-align: middle; /* [3] */
  }

  /**
   * Don't make the image fluid unless explicitly defined.
   */
  img[width],
  img[height] {
    max-width: none;
  }

    /**
   * Minimal styling for basic anchor tags
   */
  a {
    transition: color 0.3s ease,color 0.3s ease;
    color: #000000;
    text-decoration: none;
    cursor: pointer;
  }

  a:hover,
  a:focus {
    color: #292930;
    text-decoration: none;
  }

    /**
   * High-level, page-level styling.
   */
  html {
    background-color: #fff;
    
    font-size: 1rem;
    line-height: 1.25rem;
  
    font-family: HelveticaNeue,sans-serif;
    
    color: #000000;
    min-height: 100%;
    -webkit-text-size-adjust: 100%;
    -ms-text-size-adjust: 100%;
    -moz-osx-font-smoothing: grayscale;
    -webkit-font-smoothing: antialiased;
    scroll-behavior: smooth;
  }

  @media (min-width: 768px) {
    html {
      background-color: #fff;
    }
  }

    /**
   * Force all headings and semantic text elements to have the exact same styling
   * by default. Allowing freedom of cosmetic choice (via components) with the
   * correct semantic element.
   */
  h1, h2, h3, h4, h5, h6 {
    
    font-size: 1rem;
    line-height: 1.25rem;
  
    font-weight: normal;
  }

  small {
    font: inherit;
  }
  

  .o-container {
    max-width: 1312px;
    padding-right: 1rem;
    padding-left: 1rem;
    margin-right: auto;
    margin-left: auto;
  }

  .o-container--inset {
    max-width: 1280px;
  }

  /* Flush at 'medium' down */
  @media (max-width: 768px) {
    .o-container--flush\<medium {
      padding-right: 0;
      padding-left: 0;
    }
  }

  /**
   * Places any image-like content next to any text-like content.
   *
   * See: https://csswizardry.com/2013/05/the-flag-object/
   */
  .o-flag {
    display: table;
    width: 100%;
  }

  .o-flag__img,
  .o-flag__body {
    display: table-cell;
    vertical-align: middle;
  }

  .o-flag__img {
