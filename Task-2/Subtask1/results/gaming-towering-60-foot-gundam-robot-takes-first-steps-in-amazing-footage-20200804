        <script>
        document.body.addEventListener('pbBrandedElementEvent', function(event) {
          if (!event || !event.detail) { return; }

          var eventName = event.detail.name;
          var eventData = event.detail.data;

          switch(eventName) {
            case 'branded-element.engaged':
              if (eventData.pageActionType !== 'vote') { return; }

              permutive.track('PollResponse', {
                "survey": {
                  "id": eventData.elementId
                },
                "question": {
                  "id": eventData.itemId,
                  "text": eventData.itemQuestionText
                },
                "answer": {
                  "id": eventData.itemAnswerId,
                  "text": eventData.itemAnswerText
                },
                "provider": "PlayBuzz"
              });
            break;
          }
        });
</script>
