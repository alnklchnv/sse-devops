            var playerEvents = {
                onPlayerError: (player, eventName, eventData) => {
                   // console.log('Player event ', eventName, eventData);
                },
                onInit: (player, eventName, eventData) => {
                    //console.log('Player event ', eventName, eventData);
                },
                onVideoStarted: (player, eventName, eventData) => {
                    ga('send', 'event', 'Live TV', 'Play', title);
