    function adBlockMessage(cls,msg){
        var html ='';
        html += '<div class="_video_ad">';
        html += '<div class="_ad_blocker">';
        html += '<div class="_ad_blocker_inner">';
        html += '<div class="_on_blocker">You seems to have an ad blocker on</div>';
        html += '<div class="_continue_blocker">Please turn it off in order to continue</div>';
        html += '<div class="_off_blocker">';
