                                        data-runner-img-md="https://www.newsgram.com/media-library/wolves-in-captivity-in-india.jpg?id=27623089&width=600&quality=85&coordinates=0%2C26%2C0%2C27&height=400"
                                        data-runner-img-sd="https://www.newsgram.com/media-library/wolves-in-captivity-in-india.jpg?id=27623089&width=600&quality=85&coordinates=0%2C26%2C0%2C27&height=400"
                                        data-runner-style='padding-bottom: 66.6666666667%;'
