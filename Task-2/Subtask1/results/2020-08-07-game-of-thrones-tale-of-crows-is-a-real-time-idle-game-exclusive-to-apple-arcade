							appendVidoomy();
						}
						if (typeof appendSunmedia== 'function') {
							appendSunmedia();
						}
						removeOneTrustPlaceholders('targeting');
					} else {
						if (typeof appendComscore == 'function') {
							appendComscore(0);
						}
					}

					__tcfapi('getTCData', 2, (tcData, success) => {

						if(success) {
							if(tcData.purpose.consents[1]) {
								if (typeof doAdsOnConsent == 'function') {
									if (typeof appendPubstack == 'function') {
										appendPubstack();
									}
																			doAdsOnConsent();
																	}
							}
						}

					});

					showOneTrustPlaceholders();
				}
			</script>
					<script type='text/javascript'>
			function appendPubstack() {
				var scriptProto = 'https:' == document.location.protocol ? 'https://' : 'http://';
				var script = document.createElement('script');
				script.type = 'text/javascript';
				script.async = true;
				script.src = scriptProto+'boot.pbstck.com/v1/tag/6c7e4956-d33d-4bbc-bb6a-e17aa4e3ab7d';
				(document.getElementsByTagName('head')[0] || document.body).appendChild(script);
			};
			</script>
			</body>
</html>
		