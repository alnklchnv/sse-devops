    <script>
  function shouldParse(arg) {
    if (arg === undefined) {
      return null;
    }
    return typeof arg === "string" ? JSON.parse(arg) : arg;
  }
  function shouldNullify(arg) {
      return arg === undefined ? null : arg;
  }
  function shouldParseOrEmptyArr(arg) {
    if (arg === undefined) {
      return [];
    } else if (Array.isArray(arg)) {
      return arg;
    } else {
      return JSON.parse(arg);
    }
  }

  function shouldStringify(arg) {
    if (typeof arg === "string") {
      return arg;
    }
    return arg === undefined ? null : JSON.stringify(arg);
  }
</script>
