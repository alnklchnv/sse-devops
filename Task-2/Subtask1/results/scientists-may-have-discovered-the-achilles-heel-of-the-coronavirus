  // on qa1, load the staging src instead of the prod src
  // requested by HTL so they can test future changes on qa1
  function isStagingEnvironment() {
    if (window.location.hostname === "qa1.qa.theladders.com") { return true; }
    else { return false; }
  }
  window.onload = function() {
    const stagingSrc = "https://htlbid.com/stage/v3/theladders.com/htlbid.js",
          prodSrc = "https://htlbid.com/v3/theladders.com/htlbid.js"
    var htlScript = document.createElement('script');
    htlScript.defer = true;
    htlScript.setAttribute('src', isStagingEnvironment() ? stagingSrc : prodSrc);
    document.body.appendChild(htlScript);
  }
</script>

<script async>
  function isDevelopmentEnvironment() {
    if (window.location.hostname !== "www.theladders.com") { return "yes" } 
    else { return "no" }
  }

  function getEnvironment() {
    if (window.location.hostname !== "www.theladders.com") { return "development" } 
    else { return "production" }
  }

  function isOnHomepage() {
    if (window.location.pathname === "/career-advice") { return "yes" }
    else { return "no" }
  }

  function formatStringAsArray(arrayAsString) {
    const cleanedUpArray = arrayAsString.replace(/[\[\]']+/g, "")
    const arrayOfTags = cleanedUpArray.split(',').map(tag => tag.trim())
    return arrayOfTags
  }
  
  window.htlbid = window.htlbid || {};
  htlbid.cmd = htlbid.cmd || [];
  htlbid.cmd.push(function() {
    htlbid.layout('universal'); // Leave as 'universal' or add custom layout
    htlbid.setTargeting("is_testing", isDevelopmentEnvironment()); // Set to "no" for production
    htlbid.setTargeting("is_home", isOnHomepage()); // Set to "yes" on the homepage
    htlbid.setTargeting('post_id', "01-sid-KKEG4PPODWVLJ7TPYVSTT2EVKY"); // output the ID of the post, article pages only
