      $('.realated_stories_slider').bxSlider({
        pager: false,
        infiniteLoop: false,
        hideControlOnEnd: true,
        minSlides: 2.5,
        maxSlides: 2.5,
        moveSlides: 1,
        slideWidth: 600
      });
    });
  </script>
