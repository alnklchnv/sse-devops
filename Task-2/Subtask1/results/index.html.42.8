function domainNameCheck(email){
  var val    = true;
  var filter = /^([a-zA-Z0-9_\-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([a-zA-Z0-9\-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;
  if (!filter.test(email)) {
    val = false;
    return val;
  }
  
  var email  = email.toLowerCase();
  var arr    = new Array('yaahoo.com','gmial.com','yahaoo.com','gamil.com','gfgfg.com','hotmaul.com','gmail.co','shshs.com','gimal.com','gmail.cm','gmalil.com','rediffmail.cm','gmil.com','gmaul.com','redifmail.com','gmai.com','hotmal.com');
  var atpos  = email.lastIndexOf("@");
  var dotpos = email.lastIndexOf(".");
  var ext    = email.substring(atpos+1,dotpos.length);
  for(var i=0; i<arr.length; i++)
  {
    if(ext == arr[i])
    {
      val = false;
      break;
    } 
  }
  return val;
}
