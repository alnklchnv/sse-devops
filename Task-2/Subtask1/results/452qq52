                OneSignal.on('subscriptionChange', function(isSubscribed) {
                    if (isSubscribed === true) {
                        
                        OneSignal.sendTags({
                            "sub_cat": "world news",
                        }).then(function(tagsSent) {
                            
                        });
                    }
                });

                if (typeof localStorage !== "undefined") {
                    let pageVisits = parseInt(localStorage.getItem('ppvs'), 10);
                    if (!isNaN(pageVisits)) {
                        pageVisits += 1;
                    } else {
                        pageVisits = 1  ;
                    }
                    localStorage.setItem('ppvs', pageVisits)
                }

        });

        window.dataLayer = window.dataLayer || [];
        OneSignal.push(function() {
                OneSignal.on('notificationPermissionChange', function(permissionChange) {
                let currentPermission = permissionChange.to;
                
                window.dataLayer.push({
                    'event': 'permissionChangeEvent',
                    'currentPermission': currentPermission
                });
            });

            OneSignal.on('popoverAllowClick', function() {
                window.dataLayer.push({
                    'event': 'pushSlidePrompt',
                    eventCategory: "push notification",
                    eventAction: "prompt allow"
                });
            });

            OneSignal.on('popoverCancelClick', function() {
                window.dataLayer.push({
                    'event': 'pushSlidePrompt',
                    eventCategory: "push notification",
                    eventAction: "prompt no"
                });
            });

            OneSignal.on('popoverClosed', function() {
                window.dataLayer.push({
                    'event': 'pushSlidePrompt',
                    eventCategory: "push notification",
                    eventAction: "prompt close"
                });
            });
        });

        
        //art & cat
            let osTagsForPage = {"world news":0};
            function osUpdateTags(osTags) {
                OneSignal.push(function() {
                    OneSignal.isPushNotificationsEnabled(function(isEnabled) {
                    if (isEnabled) {
                        
                        OneSignal.sendTags(osTags).then(function(tagsSent) {
                        

                        });
                    }
                    });
                });
            }

            if (typeof localStorage !== "undefined") {
                let osTags = {};
                let localTags = getOSTags();
                
                let tagsUpdated = false

            for (key in osTagsForPage) {
                if (key in localTags) {
                    
                    if (!isNaN(localTags[key])) {
                        localTags[key] = (localTags[key] + 1 || 1);
                        osTags[key] = localTags[key];
                        
                        tagsUpdated = true;
                    } else {
                        
                    }

                } else {
                    
                    osTags[key] = 1;
                    
                    tagsUpdated = true;
                }
            }

            localStorage.setItem("osTags", JSON.stringify(osTags));

                if (tagsUpdated) {
                    osUpdateTags(osTags)
                }
            }

            function getOSTags() {
            return localStorage.getItem("osTags") ?
                JSON.parse(localStorage.getItem("osTags")) : [];
            }
        
    </script>


    
    
    
    

