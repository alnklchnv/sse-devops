                       delete window['cx_widgetDataArticleDetails'];
                    }
                    
                    if (isCCEDownArticleDetails) { trackStoryWhenCxenseDown(); }
                    else { setCookie('ccedown', '', -1); } // remove cookie ccedown when cce is OK

                                        //START (TSOL Paywall - Track stories when cxense down)
                    function trackStoryWhenCxenseDown() {
                        // console.log('Begin trackStoryWhenCxenseDown');
                        var content_tier = (jQuery('meta[name="content_tier"]').attr('content') || '').toLowerCase();
                        var content_id   = jQuery('meta[name="content_id"]').attr('content') || '';
                        var timestamp    = getTimeStamp();
                        var visits       = 1;
                        var ccedown      = getCookie('ccedown');
                
                        //console.log({ content_tier, content_id, timestamp, visits, ccedown });
                
                        if (content_tier === 'metered' || content_tier === 'premium') {
                            if (ccedown === null) {
                                var obj = { visits, timestamp, id: [content_id] };
                                setCookie('ccedown', JSON.stringify(obj), 1);
                            }
                            else {
                                var ccedownObj;
                                try { ccedownObj = jQuery.parseJSON(ccedown); } catch (error) { console.error('ccedown parse error'); return; }
                
                                // check time diff
                                var date1 = moment(ccedownObj['timestamp'], 'YYYY-MM-DD HH:mm').toDate();
                                var date2 = moment(timestamp, 'YYYY-MM-DD HH:mm').toDate();
                                var diff  = date2.getTime() - date1.getTime();
                                var msec  = diff;
                                var mm    = Math.floor(msec / 1000 / 60);
                                // msec -= mm * 1000 * 60;
                                // mm = 10;
                                // console.log({ date1, date2, diff, mm });
                                // console.log('condition', !ccedownObj['id'].includes(content_id) && mm < 30, mm > 30);
                                if (!ccedownObj['id'].includes(content_id) && mm < 1440) {
                                    // add visit count within 1 day
                                    ccedownObj['id'].push(content_id);
                                    ccedownObj['visits'] += 1;
                                    // console.log('setCookie case1', JSON.stringify(ccedownObj));
                                    setCookie('ccedown', JSON.stringify(ccedownObj), 1);
                                } else if (mm > 1440) { // 1 day
                                    ccedownObj['visits']    = 1;
                                    ccedownObj['timestamp'] = timestamp;
                                    ccedownObj['id']        = [content_id];
                                    // console.log('setCookie case2', JSON.stringify(ccedownObj));
                                    setCookie('ccedown', JSON.stringify(ccedownObj), 1);
                                }
                            }
                        }
                
                        // console.log('End trackStoryWhenCxenseDown');
                        
                        function getTimeStamp(){
                            var currentdate = new Date();
                        
                            var y = digitPad(currentdate.getFullYear());
                            var mon = digitPad(currentdate.getMonth()+1);
                            var d = digitPad(currentdate.getDate());
                            var h = digitPad(currentdate.getHours());
                            var min = digitPad(currentdate.getMinutes());
                            var s = digitPad(currentdate.getSeconds());
                        
                            return datetime = y+"-"+mon+"-"+d+" "+h+":"+min+":"+s;
                        }
                        
                        function digitPad(digit){
                            if (digit.toString().length == 1) {
                                digit = "0" + digit;
                            }
                            return digit;
                        }
                    }
                    //END (TSOL Paywall - Track stories when cxense down)
                    
                    let ccedownVisits = checkCCEDownCookie() || 1;
                    //console.log('ccedownVisits', ccedownVisits);
                    function checkCCEDownCookie() {
                        var ccedown = getCookie('ccedown') || '';
                        var ccedownObj;
                        if(ccedown != ''){
                            try { ccedownObj = jQuery.parseJSON(ccedown); } catch (error) { console.error('ccedown parse error'); return null; }
                            //console.log('readcookieccedown', ccedown, ccedownObj, ccedownObj['visits']);
                            return ccedownObj['visits'];
                        }else{
                            return null;
                        }
                    }
                    
                    if (window['cx_widgetDataArticleDetails']) {
                            
                    }
                    $(window).on("load", function() {
                    // render result if available
                    if (window['cx_widgetDataArticleDetails']) {
						cX.CCE.render('cx_paywall', window['cx_widgetDataArticleDetails']);
                        jQuery("#cx_paywall").removeAttr("style");
                    }
                    else if (ccedownVisits > 3 && isCCEDownArticleDetails) {
                        console.error(ccedownVisits, CCEErrorArticleDetails);
                        let html = `
                            <div id="cx_paywall_ccedown">
                                <div id="noticeDiv" class="notice-wrap">
                                    <section class="row limited-notice">
                                        <div class="col-xs-12 col-sm-8">
                                            <div class="limit-notice-desc-ccedown">
                                                <h3></h3><br>
                                                <p></p>
                                                <div class="premium-access-ccedown" style="text-align:center;"></div>
                                            </div>
                                        </div>
                                        <div class="col-xs-12 col-sm-4 paywall-access-box"><img src="https://cdn.thestar.com.my/Themes/img/trial-img.png"></div>
                                    </section>
                                </div>
                            </div>
                        `;
                        
                        jQuery('#cx_paywall').after(html);
                        let redirectUrl = 'https://sso.thestar.com.my/?lng=en&channel=1&ru=HNQ8Auw31qgZZU47ZjHUhHKJStkK3H51/pPcFdJ1gQ9cFgPiSalasDvF6DeumuZw70eO3osqfRYt9bpw91/7YQ==';
                        if (jQuery("#bmRedirect").attr("value")) { redirectUrl = jQuery("#bmRedirect").attr("value"); }
                        else if (jQuery("#btn_login .login").attr("href")) { redirectUrl = jQuery("#btn_login .login").attr("href"); }
                        let cx_paywall_layout_headline = 'Register today for a FREE trial!';
                        let cx_paywall_layout_desc = 'For 1 month only, get unlimited digital access to articles, views and customised features.';
                        let cx_paywall_layout = `
                            <a class="f14 btn btn-default freebutton" href="/subscribe/?p=trial" style="text-decoration:none; background:#18B14C; color:#fff; padding: 10px 25px; font-size: 1em; border: none;" onclick="window.dataLayer = window.dataLayer || []; window.dataLayer.push({'event': 'paywall_click','paywall_type' : 'trial'});">Try For Free</a>
                            <span class="log-desc" style="display: inline-flex; flex-flow: column; align-items: center;">
                                <a class="btn btn-primary login-button" id="cX_login" style="text-decoration:none; color:#fff;" href="`+ redirectUrl + `">Log In</a>
                                <p style="font-size:10px; margin-left:10px; color: #9b9b9b;" id="cX_login_desc">if you have a subscription</p>
                            </span>
                        `;
                        jQuery(".limit-notice-desc-ccedown h3").text(cx_paywall_layout_headline);
                        jQuery(".limit-notice-desc-ccedown p").text(cx_paywall_layout_desc);
                        jQuery(".premium-access-ccedown").html(cx_paywall_layout);
                    }
					});
                    
                    function setCookie(name,value,days) {
                        var expires = "";
                        if (days) {
                            var date = new Date();
                            date.setTime(date.getTime() + (days*24*60*60*1000));
                            expires = "; expires=" + date.toUTCString();
                        }
                        document.cookie = name + "=" + (value || "")  + expires + "; path=/";
                    }
                    function getCookie(name) {
                        var nameEQ = name + "=";
                        var ca = document.cookie.split(';');
                        for (var i = 0; i < ca.length; i++) {
                            var c = ca[i];
                            while (c.charAt(0) == ' ') c = c.substring(1, c.length);
                            if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length, c.length);
                        }
                        return null;
                    }
                </script>
