                <script id="CCERenderSticky" type="text/javascript">
                    // result from /public/widget/data wrapped in  {"response":{} }
                    window['cx_widgetDataFooter'] = { "response": {"items":[{"type":"advanced","campaign":"1","testId":"4"}],"template":"<!--%\nvar items = data.response.items;\nfor (var i = 0; i < items.length; i++) {\n    var item = items[i];\n%-->\n<!--% if (item.campaign != 1) { %-->\n<a class=\"f14\" tmp:id=\"{{cX.CCE.clickTracker(item)}}\" tmp:href=\"{{item.url}}\">\n<p style=\"text-align:center;\">{{item['V1 Header']}}\n  &nbsp;{{item['V1 Body']}}\n <!--% if (item.campaign != 17 && item.campaign != 19) { %-->\n &nbsp;{{item['V2 Header']}}\n<!--% } %-->\n<\/p><\/a>\n<!--%\n}\n}\n%-->","prnd":"kui3a9jnnrdq96wh28xb"} };
                    window['CCEAPIRequestFooter'] = { "request": {"json":{"context":{"referrer":"https:\/\/www.google.com\/","url":"https:\/\/www.thestar.com.my\/opinion\/letters\/2010\/04\/04\/claims-for-medical-expenses","categories":{"usertype":"anonymous web"},"parameters":[{"key":"usertype","value":"anonymous web"}]},"widgetId":"4e4f71fba5ec6f497e11d1a90c1078c143b0aed4","user":{"ids":{"usi":"ktepflkdxwdfw3y9"}},"prnd":"kui3a9jnnrdq96wh28xb"}} };
                    
                    let isCCEDownFooter = false;
                    let CCEErrorFooter = window['cx_widgetDataFooter'].response;
                    if (CCEErrorFooter === '5xx' || CCEErrorFooter === 'timeout' || CCEErrorFooter === '4xx' || CCEErrorFooter === 'requesttimeout') {
                       isCCEDownFooter = true;
                       delete window['cx_widgetDataFooter'];
                    }
                    
                    $(window).on("load", function() {
						// render result if available
						if (window['cx_widgetDataFooter']) {
							cX.CCE.render('cx_sticky', window['cx_widgetDataFooter']);
						}
                        if (window['cx_widgetDataFooter'] && window['cx_widgetDataFooter'].response.items && parseInt(window['cx_widgetDataFooter'].response.items[0].campaign) !== 1) {
                            loadsticky();
                        }
                        else if (isCCEDownFooter) {
                            console.error(CCEErrorFooter);
                            let html = `
                                <div id="cx_sticky_footer_ccedown">
                                    <a class="f14" href="https://www.thestar.com.my/subscribe/?p=trial">
                                        <p style="text-align:center;">Start your &nbsp;<span style="background-color:#000;color:#fff; font-weight:700; font-size:16px;padding:2px 4px;">FREE</span> &nbsp;trial today for unlimited access to content and features.</p>
                                    </a>
                                </div>
                           `;
                           $('#cx_sticky').after(html);
                           $('#promobox').show();
                        }
                    });
                </script>                            
    
<script type="text/javascript">
cX.CCE = cX.CCE || {}; cX.CCE.callQueue = cX.CCE.callQueue || [];
$(document).ready(function() {
    var sticky = $('#promobox');

    $("#promobox a.close").click(function () {
        sticky.css({'right':'-1903px'});
        sticky.fadeOut('slow');  
        setCookie('promobox', '1');
    });  
});
function loadsticky(){

    var showPromo = 1;
    var i, x, y, ARRcookies = document.cookie.split(";")
    for (i = 0; i < ARRcookies.length; i++) {
        x = ARRcookies[i].substr(0, ARRcookies[i].indexOf("="))
        y = ARRcookies[i].substr(ARRcookies[i].indexOf("=") + 1)
        x = x.replace(/^\s+|\s+$/g, "")
        if (x == "promobox")
            showPromo = 0;
    }
    
    if(showPromo == 1){
        $('#promobox').show();
        $("#cx_sticky").removeAttr("style");
    }
}
function setCookie(name, value) {
    //set 1 day 24*60*60
    value = encodeURI(value) + '; max-age=3600' 
    document.cookie = name + '=' + value + '; path=/;'
} 
</script>
</div>

