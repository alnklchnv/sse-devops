                <div class="input-group-btn">
                    <input class="btn btn-default" style="font-weight: bold" type="button" id="btnSearch"
                        value="Search"
                        onclick="
                            if ($.trim($('#qTextBox').val()).length >=3) {
                                jQuery('#qTextBox').addClass('loadinggif');
                                post('/search/', { q: document.getElementById('qTextBox').value, qsort: 'newest', qrec: '10', qstockcode: document.getElementById('qStockCodeVal').value }, 'get');
                                ga('send', 'event', 'TSOL NAVI', 'Extra-Panel', 'Search Button');
                            }else if($.trim($('#qTextBox').val()).length > 0 && $.trim($('#qTextBox').val()).length <3){
                                jQuery('#qTextBox').removeClass('loadinggif');
                                alert('Please enter at least 3 characters for search phrase.');
                            }else{
                                jQuery('#qTextBox').removeClass('loadinggif');
                                alert('Please enter a search phrase.');
                            }
                            " />
                    <a href="/search?adv=1" class="btn btn-default adv-search-link"
                        onclick="ga('send', 'event' , 'TSOL NAVI' , 'Extra-Panel' , 'Advance Search' ); "
                        style="font-weight: 900" data-list-type="Header" data-content-type="Navigation"
                        data-content-title="Advance Search"
                        data-content-id="https://www.thestar.com.my/search/?adv=1" onmousedown="searchAdvanceClick(this,'qTextBox')"><i class="icon sprite-icon plus"></i>
