<div class="l-bl dfp-advertise-block block block-config-provider--dfp block-plugin-id--dfp-ad-365e010b-67df-4707-9494-71c7796152e0">
  <div class="block__inner">

    <div class="block__content">
<div id="js-dfp-tag-trimed_mobile_dynamic_b2">
        <script type="text/javascript">
        googletag.cmd.push(function() {
            googletag.display('js-dfp-tag-trimed_mobile_dynamic_b2');
        });
    </script>
