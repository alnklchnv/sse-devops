    function enableTheme(newTheme = 'light') {
        let body = document.getElementsByTagName("BODY")[0];
        let moonImg = document.getElementsByClassName("dark_mode_switcher_image")[0];
        let moonImgDesktop = document.getElementsByClassName("dark_mode_switcher_image")[1];

        if (body.classList.contains('dark-mode')) {
            body.classList.remove('dark-mode');
            newTheme = 'light';
            moonImg.src = "https://cdn.pocket-lint.com/bundles/pocketlintapp/images/moon.svg";
            moonImgDesktop.src = "https://cdn.pocket-lint.com/bundles/pocketlintapp/images/moon.svg";
        } else {
            moonImg.src = "https://cdn.pocket-lint.com/bundles/pocketlintapp/images/moon_dark.svg";
            moonImgDesktop.src = "https://cdn.pocket-lint.com/bundles/pocketlintapp/images/moon_dark.svg";
            body.classList.add('dark-mode');
            newTheme = 'dark-mode';
        }

        window.setCookieLocal("color_mode", newTheme, 365);
    }

