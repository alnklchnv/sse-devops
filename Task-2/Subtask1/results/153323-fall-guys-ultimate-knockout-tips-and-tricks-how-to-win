    function cloneAttributes(element, sourceNode) {
        let attr;
        let attributes = Array.prototype.slice.call(sourceNode.attributes);
        while (attr = attributes.pop()) {
            element.setAttribute(attr.nodeName, attr.nodeValue);
        }
    }

    function jQueryLoaded() {
        consoleLog("jQuery loaded", $);
        jQuery = $ || null;
        window.jQuery = jQuery;
        loadGtmCode();

        loadStyleSheetWithExtras("https://cdn.pocket-lint.com/media/article_16.css")
        loadStyleSheetWithExtras("https://cdn.pocket-lint.com/media/complement_article_12.css")
        loadStyleSheetWithExtras("https://cdn.pocket-lint.com/media/dark_mode_14.css")
        loadStyleSheetWithExtras("https://cdn.pocket-lint.com/bundles/pocketlintapp/slick/slick.min.css")
        loadStyleSheetWithExtras("/bundles/pocketlintapp/slick/slick-theme.min.css")

        loadScriptWithExtras(slick_script_url)
            .then(loadSlick, failureCallback);

                if (showPlAds) {
            initAds();
        }
        
        loadScriptWithExtras("https://cdn.pocket-lint.com/bundles/pocketlintapp/js/swiper/swiper-bundle.min.js", null).then(function () {
            var swContainers = document.querySelectorAll(".swiper-container")
            for (var i = 0; i < swContainers.length; i++) {


                const isNativeBoxSlider = swContainers[i].parentElement.parentElement.attributes.id && swContainers[i].parentElement.parentElement.attributes.id.value === "hub";

                const swiper = new Swiper(swContainers[i], {
                    loop: false,
                    navigation: {
                        nextEl: document.querySelectorAll('.swiper-button-next')[i],
                        prevEl: document.querySelectorAll('.swiper-button-prev')[i],
                    },
                    pagination: {
                        el: document.querySelectorAll('.swiper-pagination')[i],
                        type: 'fraction',
                    },
                });

                if (!isNativeBoxSlider) {
                    handleOnSlideChange(swiper, i)
                }

                document.querySelectorAll('.swiper-button-next')[i].style.display = "block";
                document.querySelectorAll('.swiper-button-prev')[i].style.display = "block";
                if (document.querySelector('#numbering')) {
                    document.querySelector('#numbering').style.display = "none";
                }
            }


        })

        activateScrollDetect();
    }

    function closeNonUsedSlots() {
        var targetSlots = ['Interscroller', 'Skin'] //array - Slots to check


                /*var chatBubbleOrderId =  //number Order ID for chat bubble on live site*/
        var idsListToPreventCampaign = window.idsListToPreventCampaign || [2744647893, 2692919413, 2747769802];
        
        var idsListToPreventAdvertiser = window.idsListToPreventAdvertiser || [101629442];


        googletag.cmd.push(function () {
            googletag.pubads().addEventListener('slotRenderEnded', function (e) {
                var slot = e.slot.getSlotElementId()
                targetSlots.forEach(function (targetSlot) {

                    document.getElementById(slot).style.backgroundColor = "rgb(16 16 16 / 0%)";

                                        if (slot === targetSlot) {
                        document.getElementById("top-skin-ad-bg").style.backgroundColor = "rgb(16 16 16 / 0%)";
                        if (document.getElementById("pre-interscroller")) {
                            document.getElementById("pre-interscroller").style.backgroundColor = "rgb(16 16 16 / 0%)";
                        }

                        
                        if (slot === "Skin") {
                            if (typeof window.onscrollInterscroller === "undefined") {
                                loadInterscroller();
                            } else {
                                window.onscrollInterscroller();
                            }

                            if (e.isEmpty || idsListToPreventCampaign.indexOf(e.campaignId) >= 0 || idsListToPreventAdvertiser.indexOf(e.advertiserId) >= 0) {
                                //document.getElementById("top-skin-ad-bg").style.display = "none";
                                //document.getElementById("top-skin-ad").style.display = "none";
                                                            } else {
                                document.getElementById("top-skin-ad-bg").style.display = "block";
                                document.getElementById("top-skin-ad").style.display = "block";



                                                            }
                        } else {
                            if (e.isEmpty || idsListToPreventCampaign.indexOf(e.campaignId) >= 0 || idsListToPreventAdvertiser.indexOf(e.advertiserId) >= 0) {
                                //document.getElementById("top-skin-ad-bg").style.display = "none";
                                document.getElementById("top-skin-ad").style.display = "block";
                                document.getElementsByClassName("block block-inline block-mpu-middle-text im-mobi111le-div interscroller-ad-bar")[0].style.display = "block";
                                document.getElementById("banner-core").style.backgroundColor = "rgb(16 16 16 / 0%)";
                                                            } else {
                                document.getElementById("top-skin-ad-bg").style.display = "block";
                                document.getElementById("top-skin-ad").style.display = "block";
                                document.getElementsByClassName("block block-inline block-mpu-middle-text im-mobi111le-div interscroller-ad-bar")[0].style.display = "block";
                                                            }
                        }
                    }
                })
            });
        });
    }

    function populateContent(src, div) {

        var xhttp = new XMLHttpRequest();
        xhttp.onreadystatechange = function () {
            if (this.readyState == 4 && this.status == 200) {
                //var data = JSON.parse(this.responseText);
                var data = this.responseText;

                div.insertAdjacentHTML("beforeend", data);
                loadRevContent();
            }
        };
        xhttp.open('GET', src, true);
        xhttp.send();
    }


</script>
