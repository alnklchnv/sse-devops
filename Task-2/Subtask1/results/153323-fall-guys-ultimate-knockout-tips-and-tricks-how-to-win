        getCurrentColorMode();

        if (color_mode) {
            if (color_mode && color_mode == 'dark-mode') {
                document.getElementById("dark_mode_switcher_image").src = "https://cdn.pocket-lint.com/bundles/pocketlintapp/images/moon_dark.svg";
                document.getElementById("dark_mode_switcher_image_mobile").src = "https://cdn.pocket-lint.com/bundles/pocketlintapp/images/moon_dark.svg";
            } else {
                document.getElementById("dark_mode_switcher_image").src = "https://cdn.pocket-lint.com/bundles/pocketlintapp/images/moon.svg";
                document.getElementById("dark_mode_switcher_image_mobile").src = "https://cdn.pocket-lint.com/bundles/pocketlintapp/images/moon.svg";
            }
        }

        if (showPlAds) {

            loadScriptWithExtras("https://cdn.jsdelivr.net/gh/leya/leyajs/dist/main.js", ["defer", "true"])
                .then(sendTagsToLeya);

        }

        loadScriptWithExtras("https://www.google-analytics.com/analytics.js", null);
        loadScriptWithExtras("https://static.chartbeat.com/js/chartbeat.js", null);
        
    }

