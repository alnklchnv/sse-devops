        function loadSkinCode() {

        }

        function sendToAnalytics({name, delta, id}) {
            consoleLog("send To analytics " + id + " -> " + name + " ( " + ( Math.round(name === 'CLS' ? delta * 1000 : delta) )  + " ) " );

            if(typeof window.ga !== "undefined")
            {
                window.ga('unifiedTracker.send', 'event', {
                    eventCategory: 'Web Vitals',
                    eventAction: name,
                    eventLabel: id,
                    eventValue: Math.round(name === 'CLS' ? delta * 1000 : delta),
                    nonInteraction: true,
                    transport: 'beacon',
                });
            }
        }
    </script>
<script defer src="https://unpkg.com/web-vitals"></script>


