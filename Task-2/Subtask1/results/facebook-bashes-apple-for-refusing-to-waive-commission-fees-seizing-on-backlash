            <!-- Related A/B Testing -->
                            <script type="text/javascript">
                    $(document).ready(function(){
                        if ($(window).width() >= 1024) {
                            $.get("https://api.cxense.com/public/widget/data?json=%7B%22widgetId%22%3A%2254aaff126eb002491eaa07aecde1d51cee28be16%22%2C%22user%22%3A%7B%22ids%22%3A%7B%22usi%22%3A%22%22%7D%7D%2C%22prnd%22%3A%22kui4v5evxse88bu2udqs%22%7D",
                            function(data ,status){
                                if (data.items[0].title) {
                                    if (data.items[0].title == 'related-stories-side-div') {
                                        if ($('#related-stories-resp-div').length) {
                                            $('#related-stories-resp-div').hide();
                                        }  
                                        if ($('#related-stories-div').length) {
                                            $('#related-stories-div').hide();
                                        }
                                    } 
                                    else {  
                                        if ($('#related-stories-resp-div').length) {
                                            $('#related-stories-resp-div').hide();
                                        }  
                                        if ($('#related-stories-side-div').length) {
                                            $('#related-stories-side-div').hide();
                                        }
                                    }   
                                }
                                else {
                                    if ($('#related-stories-div').length) {
                                        $('#related-stories-div').hide();
                                    }  
                                    if ($('#related-stories-side-div').length) {
                                        $('#related-stories-side-div').hide();
                                    }
                                    if ($('#related-stories-resp-div').length) {
                                        $('#related-stories-resp-div').hide();
                                    }
                                }
                            })
                            .fail(function(xhr, status, error) {
                                if ($('#related-stories-div').length) {
                                    $('#related-stories-div').hide();
                                }  
                                if ($('#related-stories-side-div').length) {
                                    $('#related-stories-side-div').hide();
                                }
                                if ($('#related-stories-resp-div').length) {
                                    $('#related-stories-resp-div').hide();
                                }
                            });
                        }
                        else {
                            if ($('#related-stories-div').length) {
                                $('#related-stories-div').hide();
                            }  
                            if ($('#related-stories-side-div').length) {
                                $('#related-stories-side-div').hide();
                            }
                        }
                      });
                </script>
            <!-- /Related A/B Testing -->
                
