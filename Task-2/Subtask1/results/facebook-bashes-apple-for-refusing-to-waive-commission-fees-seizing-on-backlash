<script type="text/javascript">
    //JS specifically for assigning title to the navigation bar when scrolling down and up
    var nav_headline_html = '<li class="nav__headline" style="display: none;">';
    nav_headline_html += '<a class="nav__headline__logo" href="/">';
    nav_headline_html += '<img src="https://cdn.thestar.com.my/Themes/img/logo-tsol-full.svg" alt="The Star Logo - Home">';
    nav_headline_html += '</a>';
    nav_headline_html += '<span>';
    nav_headline_html += "Facebook bashes Apple for refusing to waive commission fees, seizing on backlash";
    nav_headline_html += '</span>';
    nav_headline_html += '</li>';
    
    var nav_interact_html = '<div class="navbar__interact" style="display:none;">';
    if (jQuery("#bookmark_web").hasClass("active") || jQuery("#bookmark_mobile").hasClass("active"))
        nav_interact_html += '<div class="nav__button button--bookmark active" id="bookmark_menu">';
    else
        nav_interact_html += '<div class="nav__button button--bookmark" id="bookmark_menu">';
