    nav_interact_html += '</div>';
    nav_interact_html += '</div>';
    
    jQuery(document).ready(function () {
        jQuery(nav_headline_html).insertAfter('.show-sd');
        jQuery(nav_interact_html).insertBefore('.dropdown--subscribe');
        
        var affixElement = '#navbar-main';
        
        //Initial setup after page loaded
        if (jQuery(affixElement).hasClass("affix-top")) {
            affixSetup2(true);
        }
        else {
            affixSetup2(false);
        }
        //Scroll down/up affix (Navigation menu)
        jQuery(affixElement).on('affix.bs.affix', function () {
             affixSetup2(false);
        });
        jQuery(affixElement).on('affixed-top.bs.affix', function () {
             affixSetup2(true);
        });
    });
    
    function affixSetup2(affixTop) {
        if (!isMobile()) {
            if (affixTop) {
                jQuery(".navbar-left .dropdown").attr("style", "display: block !important");
                jQuery("#navi-home").parent().attr("style", "display: block !important");
                jQuery(".nav__headline").attr("style", "display: none !important");
                jQuery(".navbar__interact").attr("style", "display: none !important");
                //jQuery(".navbar__interact .button--share").attr("style", "display: none !important");
            } else {
                jQuery(".navbar-left .dropdown").attr("style", "display: none !important");
                jQuery("#navi-home").parent().attr("style", "display: none !important");
                jQuery(".nav__headline").fadeIn(400);
                jQuery(".navbar__interact").fadeIn(400); 
                //jQuery(".navbar__interact .button--share").fadeIn(400);
            }
        } else {
            if (affixTop) {
                jQuery(".btn--subscribe").fadeIn(400);
                jQuery(".navbar__interact").attr("style", "display: none !important");
                jQuery(".navbar-left .dropdown").attr("style", "display: block !important");
                jQuery("#navi-home").parent().attr("style", "display: block !important");
                jQuery(".nav__headline").attr("style", "display: none !important");
            } else {
                jQuery(".btn--subscribe").attr("style", "display: none !important");
                jQuery(".navbar__interact").fadeIn(400);
                jQuery(".navbar-left .dropdown").attr("style", "display: none !important");
                jQuery("#navi-home").parent().attr("style", "display: none !important");
                jQuery(".nav__headline").attr("style", "display: block !important");
            }
        }
    }
    
    function controlShareModal () {
        $('.modalDialog').toggleClass('toggleModalDialog');
    }
</script>
<script>
   var timer = [];
   var timerIncrementInt = [];
   window.addEventListener('cpeready', ({
     detail: {
       players
     }
   }) => {
     var data = [];
     var playerIds = [];

     for (i = 0; i < players.length; i++) {
       var player = players[i];
       data.push({
         "playerId": player.id,
         "playerAction": "NA",
         "playerTimer": 0,
         "contentLanguage": "NA",
         "contentDate": "NA",
         "contentTags": "",
         "contentDuration": "NA",
         "contentFormat": "NA",
         "currentVideoId": "",
         "autoplayed": "0",
         "paused": false,
         "progress25": false,
         "progress50": false,
         "progress75": false
       });
       playerIds.push(player.id);

       player.addEventListener('video_start', function(e) {
         data[playerIds.indexOf(e.target.id)]["playerTimer"] = 0;
         data[playerIds.indexOf(e.target.id)] = setDailymotionDLPushValues(e, "video_start", data[playerIds.indexOf(e.target.id)]);
         timer[playerIds.indexOf(e.target.id)] = 0;
         if (timerIncrementInt[playerIds.indexOf(e.target.id)] === undefined || timerIncrementInt[playerIds.indexOf(e.target.id)] == false) {
             timerIncrementInt[playerIds.indexOf(e.target.id)] = setInterval(function() {
                increment(playerIds.indexOf(e.target.id));
              }, 1000);
         }
       });

       player.addEventListener('video_end', function(e) {
         data[playerIds.indexOf(e.target.id)]["playerTimer"] = timer[playerIds.indexOf(e.target.id)];
         data[playerIds.indexOf(e.target.id)] = setDailymotionDLPushValues(e, "video_end", data[playerIds.indexOf(e.target.id)]);
         timer[playerIds.indexOf(e.target.id)] = 0;
         clearInterval(timerIncrementInt[playerIds.indexOf(e.target.id)]);
         timerIncrementInt[playerIds.indexOf(e.target.id)] = false;
       });

       player.addEventListener('play', function(e) {
         if (data[playerIds.indexOf(e.target.id)]["paused"]) {
           data[playerIds.indexOf(e.target.id)]["playerTimer"] = 0;
           data[playerIds.indexOf(e.target.id)] = setDailymotionDLPushValues(e, "play", data[playerIds.indexOf(e.target.id)]);
           timer[playerIds.indexOf(e.target.id)] = 0;
           timerIncrementInt[playerIds.indexOf(e.target.id)] = setInterval(function() {
             increment(playerIds.indexOf(e.target.id));
           }, 1000);
         }
       });

       player.addEventListener('pause', function(e) {
         data[playerIds.indexOf(e.target.id)]["playerTimer"] = timer[playerIds.indexOf(e.target.id)];
         data[playerIds.indexOf(e.target.id)] = setDailymotionDLPushValues(e, "pause", data[playerIds.indexOf(e.target.id)]);
         timer[playerIds.indexOf(e.target.id)] = 0;
         clearInterval(timerIncrementInt[playerIds.indexOf(e.target.id)]);
         timerIncrementInt[playerIds.indexOf(e.target.id)] = false;
       });

       player.addEventListener('timeupdate', function(e) {
         if (e.target.currentTime >= e.target.duration / 4 * 3) {
           //Progress 75%  
           if (!data[playerIds.indexOf(e.target.id)]["progress75"]) {
             data[playerIds.indexOf(e.target.id)]["playerTimer"] = timer[playerIds.indexOf(e.target.id)];
             data[playerIds.indexOf(e.target.id)] = setDailymotionDLPushValues(e, "progress75", data[playerIds.indexOf(e.target.id)]);
             timer[playerIds.indexOf(e.target.id)] = 0;
           }
         } else if (e.target.currentTime >= e.target.duration / 4 * 2) {
           //Progress 50% 
           if (!data[playerIds.indexOf(e.target.id)]["progress50"]) {
             data[playerIds.indexOf(e.target.id)]["playerTimer"] = timer[playerIds.indexOf(e.target.id)];
             data[playerIds.indexOf(e.target.id)] = setDailymotionDLPushValues(e, "progress50", data[playerIds.indexOf(e.target.id)]);
             timer[playerIds.indexOf(e.target.id)] = 0;
           }
         } else if (e.target.currentTime >= e.target.duration / 4) {
           //Progress 25%
           if (!data[playerIds.indexOf(e.target.id)]["progress25"]) {
             data[playerIds.indexOf(e.target.id)]["playerTimer"] = timer[playerIds.indexOf(e.target.id)];
             data[playerIds.indexOf(e.target.id)] = setDailymotionDLPushValues(e, "progress25", data[playerIds.indexOf(e.target.id)]);
             timer[playerIds.indexOf(e.target.id)] = 0;
           }
         }
       });

       player.addEventListener('ad_start', function(e) {
         data[playerIds.indexOf(e.target.id)]["playerTimer"] = timer[playerIds.indexOf(e.target.id)];
         data[playerIds.indexOf(e.target.id)] = setDailymotionDLPushValues(e, "ad_start", data[playerIds.indexOf(e.target.id)]);
         timer[playerIds.indexOf(e.target.id)] = 0;
       });

       player.addEventListener('ad_end', function(e) {
         data[playerIds.indexOf(e.target.id)]["playerTimer"] = timer[playerIds.indexOf(e.target.id)];
         data[playerIds.indexOf(e.target.id)] = setDailymotionDLPushValues(e, "ad_end", data[playerIds.indexOf(e.target.id)]);
         timer[playerIds.indexOf(e.target.id)] = 0;
       });
     }
   });

   function setDailymotionDLPushValues(e, eventName, data) {
     if ($(e.target).parents('.inline-img').length) {
       data["contentFormat"] = "Standalone";
     } else {
       data["contentFormat"] = "Embedded";
     }

     if (data["currentVideoId"] == e.target.video.videoId) {
       data = setDailymotionDLPushEvent(e, eventName, data);
     } else {
       $.ajax({
         url: '/controller/dailymotion/' + e.target.video.videoId,
         type: "GET",
         contentType: "application/json; charset=utf-8",
         dataType: "json",
         success: function(response) {
           data["currentVideoId"] = e.target.video.videoId;
           data["contentLanguage"] = response.language;
           data["contentDate"] = response.formatted_date;
           data["contentTags"] = response.tags.join();
           data["contentDuration"] = response.duration;

           data = setDailymotionDLPushEvent(e, eventName, data);
         }
       });
     }
     return data;
   }

   function setDailymotionDLPushEvent(e, eventName, data) {
       if (eventName == "video_start") {
         data["progress25"] = false;
         data["progress50"] = false;
         data["progress75"] = false;
         if (data["autoplayed"] == "0") {
           data["playerAction"] = "Autoplay";
           dailymotionDLPush(data["playerAction"], data["playerTimer"], e.target.video.title, e.target.video.videoId, data["contentLanguage"], data["contentDate"], data["contentTags"], data["contentDuration"], data["contentFormat"]);
           data["autoplayed"] = "1";
         } else {
           data["playerAction"] = "Play";
           dailymotionDLPush(data["playerAction"], data["playerTimer"], e.target.video.title, e.target.video.videoId, data["contentLanguage"], data["contentDate"], data["contentTags"], data["contentDuration"], data["contentFormat"]);
         }
         data["playerAction"] = "Progress 0%";
         dailymotionDLPush(data["playerAction"], data["playerTimer"], e.target.video.title, e.target.video.videoId, data["contentLanguage"], data["contentDate"], data["contentTags"], data["contentDuration"], data["contentFormat"]);
       } else if (eventName == "video_end") {
         data["playerAction"] = "Progress 100%";
         dailymotionDLPush(data["playerAction"], data["playerTimer"], e.target.video.title, e.target.video.videoId, data["contentLanguage"], data["contentDate"], data["contentTags"], data["contentDuration"], data["contentFormat"]);
       } else if (eventName == "play") {
         data["playerAction"] = "Play";
         dailymotionDLPush(data["playerAction"], data["playerTimer"], e.target.video.title, e.target.video.videoId, data["contentLanguage"], data["contentDate"], data["contentTags"], data["contentDuration"], data["contentFormat"]);
         data["paused"] = false;
       } else if (eventName == "pause") {
         data["playerAction"] = "Stop";
         dailymotionDLPush(data["playerAction"], data["playerTimer"], e.target.video.title, e.target.video.videoId, data["contentLanguage"], data["contentDate"], data["contentTags"], data["contentDuration"], data["contentFormat"]);
         data["paused"] = true;
       } else if (eventName == "progress25") {
         data["playerAction"] = "Progress 25%";
         dailymotionDLPush(data["playerAction"], data["playerTimer"], e.target.video.title, e.target.video.videoId, data["contentLanguage"], data["contentDate"], data["contentTags"], data["contentDuration"], data["contentFormat"]);
         data["progress25"] = true;
       } else if (eventName == "progress50") {
         data["playerAction"] = "Progress 50%";
         dailymotionDLPush(data["playerAction"], data["playerTimer"], e.target.video.title, e.target.video.videoId, data["contentLanguage"], data["contentDate"], data["contentTags"], data["contentDuration"], data["contentFormat"]);
         data["progress50"] = true;
       } else if (eventName == "progress75") {
         data["playerAction"] = "Progress 75%";
         dailymotionDLPush(data["playerAction"], data["playerTimer"], e.target.video.title, e.target.video.videoId, data["contentLanguage"], data["contentDate"], data["contentTags"], data["contentDuration"], data["contentFormat"]);
         data["progress75"] = true;
       } else if (eventName == "ad_start") {
         data["playerAction"] = "Ad Progress 0%";
         dailymotionDLPush(data["playerAction"], data["playerTimer"], e.target.video.title, e.target.video.videoId, data["contentLanguage"], data["contentDate"], data["contentTags"], data["contentDuration"], data["contentFormat"]);
       } else if (eventName == "ad_end") {
         data["playerAction"] = "Ad Progress 100%";
         dailymotionDLPush(data["playerAction"], data["playerTimer"], e.target.video.title, e.target.video.videoId, data["contentLanguage"], data["contentDate"], data["contentTags"], data["contentDuration"], data["contentFormat"]);
       }
       
       return data;
   }

   function dailymotionDLPush(playerAction, playerTimer, title, id, language, date, tags, duration, format) {
     dataLayer.push({
       "event": "playerEvent",
       "player_action": playerAction,
       "player_timer": playerTimer.toString(),
       "content_type": "Video",
       "content_title": title,
       "content_id": id,
       "content_category": "NA",
       "content_author": "NA",
       "content_sp": "NA",
       "content_byline": "NA",
       "content_agency": "NA",
       "content_language": language,
       "content_date": date,
       "content_tags": tags,
       "content_duration": duration.toString(),
       "content_format": format
     });
   }

   function increment(i) {
     timer[i] = timer[i] + 1;
   }
</script>

<script type="text/javascript">
var addthis_position = "";
$(document).ready(function() {
    $(".share-on-nav").click(function () {
        addthis_position = "nav";
    });
    $(".share-on-body").click(function () {
        addthis_position = "body";
    });
    
    function addThisEventHandler(evt) { 
        var addthis_position_name = "";
        if (addthis_position == "nav")
            addthis_position_name = "top";
        else if (addthis_position == "body")
            addthis_position_name = "content";
        switch (evt.type) {
            case "addthis.menu.share":
                dataLayer.push({
                    'event': 'addthis',
                    'socialPosition': addthis_position_name,
                    'socialNetwork': evt.data.service,
                    'socialAction': 'Share',
                    'socialTarget': evt.data.url
                });
                break;
        }
            }
    addthis.addEventListener('addthis.menu.share', addThisEventHandler);
    });
</script>
