)}}\" tmp:href=\"{{item.url}}\" onclick=\"window.dataLayer = window.dataLayer || []; window.dataLayer.push({'event': 'paywall_click','paywall_type' : 'trial'});\">{{item.buttonleft}}<\/a>          \n          <!--% } %-->\n          <!--% if(regresult == 0 && mixresult == 0) { %--> \n          <span style=\"display: inline-flex; flex-flow: column; align-items: center;\">\n          \t\t<a class=\"btn btn-primary login-button\" id=\"cX_login\" style=\"text-decoration:none; color:#fff;\" tmp:href=\"{{item.buttonrighturl}}\">{{item.buttonright}}<\/a>\n          \t\t<p style=\"font-size:10px; margin-left: 10px; color: #9b9b9b;\">if you have a subscription<\/p>\n          <\/span>\n          <!--% } else if (mixresult == 1) {%-->\n            <span style=\"display: inline-flex; flex-flow: column; align-items: center;\">\n          \t\t<a class=\"btn btn-primary login-button\" id=\"cX_login\" style=\"text-decoration:none; color:#fff;display:none;\" tmp:href=\"{{item.buttonrighturl}}\">{{item.buttonright}}<\/a>\n          \t\t<p style=\"font-size:10px; margin-left:10px; color: #9b9b9b; display:none;\" id=\"cX_login_desc\">if you have a subscription<\/p>\n          <\/span> \n           <!--% } %-->\n        <\/div>\n      <\/div>\n    <\/div>   \n    <div class=\"col-xs-12 col-sm-4 paywall-access-box\"><img src=\"https:\/\/cdn.thestar.com.my\/Themes\/img\/aa_star2021_v2.jpg\"><\/div>\n  <\/section>\n<\/div>\n<!--%\n}\n%-->","prnd":"kui4v5evxse88bu2udqs"} };
                    window['CCEAPIRequestArticleDetails'] = { "request": {"json":{"context":{"referrer":"","url":"https:\/\/www.thestar.com.my\/tech\/tech-news\/2020\/08\/15\/facebook-bashes-apple-for-refusing-to-waive-commission-fees-seizing-on-backlash","categories":{"usertype":"anonymous web"},"parameters":[{"key":"usertype","value":"anonymous web"}]},"widgetId":"865dc30ad99ebfb135938a902061c2c0564411f6","user":{"ids":{"usi":""}},"prnd":"kui4v5evxse88bu2udqs"}} };
                    
                    let isCCEDownArticleDetails = false;
                    let CCEErrorArticleDetails = window['cx_widgetDataArticleDetails'].response;
                    if (CCEErrorArticleDetails === '5xx' || CCEErrorArticleDetails === 'timeout' || CCEErrorArticleDetails === '4xx' || CCEErrorArticleDetails === 'requesttimeout') {
                       isCCEDownArticleDetails = true;
                       delete window['cx_widgetDataArticleDetails'];
