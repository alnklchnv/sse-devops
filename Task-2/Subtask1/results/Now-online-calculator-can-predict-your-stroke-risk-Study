<script type="text/javascript">

    function Update_me(feed_id, div_id) {

        var xhttp = new XMLHttpRequest();
        xhttp.onreadystatechange = function () {
            if (this.readyState == 4 && this.status == 200) {
                document.getElementById("div_" + div_id).innerHTML = this.responseText;
            }
        };
        xhttp.open("GET", "https://menafn.com/mgmt/mgmtnet/html_update_ajax.aspx?html_id=" + feed_id, true);
        xhttp.send();




    }


    $(document).ready(function () {

        LoadStocksAndCurrencies()
        var updateInterval = setInterval(function () {
            $.ajaxSetup({ cache: false });
            LoadStocksAndCurrencies()
        }, 3600 * 1000);
    });
    function LoadStocksAndCurrencies() {
        $("#livestockfeed").load("/includes/index/ind_live_stock_feed.aspx",
             function (response, status, xhr) {
                 if (status == "error") {
                     var msg = "Sorry but there was an error: ";
                     $("#livestockfeed").html(msg + xhr.status + " " + xhr.statusText);
                 }
             });



        $("#live_curr").load("/includes/index/ind_live_curr_feed.aspx",
            function (response, status, xhr) {
                if (status == "error") {
                    var msg = "Sorry but there was an error: ";
                    $("#live_curr").html(msg + xhr.status + " " + xhr.statusText);
                }
            });
    }
</script>

    
