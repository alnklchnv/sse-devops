<script>
  document.addEventListener('DOMContentLoaded', function() {
    if (wordpress.locale !== 'en_asia') {
      return;
    }
    getCountry().then(function(countryName) {
      if (!countryName.match(/Singapore|Malaysia|Philippines/)) {
        return;
      }
      document.querySelectorAll('.store-icon-container').forEach((storeIconContainerEl) => {
        storeIconContainerEl.style.display = 'block';
      });
    });
  });
</script>
