<meta itemprop="headline" content="WhatsApp rolling out new advanced search feature for some beta users on Android" />

<div itemprop="publisher" itemscope itemtype="https://schema.org/Organization">
    <div itemprop="logo" itemscope itemtype="https://schema.org/ImageObject">
        <meta itemprop="url" content="https://www.neowin.net/images/orion/icons/favicon-196x196.png">
