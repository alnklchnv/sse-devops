            var openwebAlreadyExists = document.querySelector('[data-spotim-module="spotim-launcher"][data-live-blog="true"][data-live-event-code]');
            var openwebLoaded = false;
            window.addEventListener('load', function(){
                document.addEventListener('scroll', openWebScript);
                document.addEventListener('mousemove', openWebScript);
            })

            function openWebScript() {
                if (!openwebLoaded && !openwebAlreadyExists) {
                    openwebLoaded = true;
                    document.removeEventListener('scroll', openWebScript);
                    document.removeEventListener('mousemove', openWebScript);
                    var script1 = document.querySelector('[data-post-id="02R2jK8K229l7BQSAzYcDUV"]');
                    var openweb = document.createElement('script');
                    openweb.src = "https://launcher.spot.im/spot/sp_K16VHJZS";
                    openweb.setAttribute('data-spotim-module', 'spotim-launcher');
                    openweb.setAttribute('data-post-url', 'https://www.pcmag.com/news/twitch-prime-is-now-called-prime-gaming');
