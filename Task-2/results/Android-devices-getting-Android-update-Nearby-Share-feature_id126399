<script type='text/javascript'>
	googletag.cmd.push(googletagCallback);
	function googletagCallback() {
		googletag.pubads().addEventListener('slotRenderEnded', function (tag) {
			var id = tag.slot.getSlotElementId(),
				ad = document.getElementById(id),
				element = findSpotDisclaimer(ad),
				className = element.className;
			if (!tag.size) {
				if (element && element.classList.contains('ad_339_143')) {
					element.remove();
				}
				return false;
			}
			if (element) {
				element.className = className += ' ad_' + tag.size[0] + '_' + tag.size[1];
			}
		}
										   );
	}
	function findSpotDisclaimer(element) {
		var parent = element.parentElement;
		if (parent.tagName.toLowerCase() === 'body') {
			return false;
		}
		if (parent.className.match('spot_disclaimer')) {
			return parent;
		}
		return findSpotDisclaimer(parent)
	}
</script>
