   (function() {	
    var linkContainerSelector = '#privacy-link-container';	
        
    function injectCCPALink() {	
        if (window.ccpaAppliesPromise && window.__uspapi) {	
        window.ccpaAppliesPromise.then(function(ccpaApplies) {	
            if (!ccpaApplies) {	
            return;	
            }	
                
            //var ccpaLinkContainer = document.querySelector(linkContainerSelector);
            var previousLi = document.querySelector('a[href="/terms-and-conditions"]').closest('li');
            var ccpaLinkContainer = previousLi.cloneNode(true);
            if (!ccpaLinkContainer) {	
            return;	
            }	
                
            //var ccpaLink = document.createElement('a');	
            var ccpaLink = ccpaLinkContainer.querySelector('a');
            ccpaLink.href = '#';	
            ccpaLink.innerText = 'Do not sell my info';	
            
            ccpaLink.addEventListener('click', function(event) {	
            event.preventDefault();	
            window.__uspapi('displayUspUi');	
            return false;	
            });
            previousLi.insertAdjacentElement('afterEnd', ccpaLinkContainer);
