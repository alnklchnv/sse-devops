<script type="text/javascript" async="async">
  var $ = jQuery;
  $(document).ready(function () {
    $.getJSON("https://getapi.indiatvnews.com/trending/live_indiatv_trending.json", function (result) {
      if (result.length > 0) {
        var content = '';
        $.each(result, function (i, field) {
          content += '<a href="' + field.url + '" title="' + field.keyword + '">' + field.keyword + '</a>';
        });
        content = '<div class="trending-link"><div class="wrapper"><span>Trending</span>' + content + '</div></div>';
        $('.twidget').html(content);
      }

    });
  });
</script>
