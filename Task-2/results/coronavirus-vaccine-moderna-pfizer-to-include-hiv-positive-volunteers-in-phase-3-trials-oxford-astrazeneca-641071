<script type="text/javascript" async="async">
  $(document).ready(function () {
    $(function () {
      $("img.lazy").lazyload({effect: "fadeIn", failure_limit: 50});
    }), $('img').error(function () {
      $(this).attr('src', 'https://static.indiatvnews.com/ins-web/images/lazy.jpg');
