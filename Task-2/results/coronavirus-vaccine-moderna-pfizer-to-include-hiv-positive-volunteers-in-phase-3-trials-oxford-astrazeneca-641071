  setTimeout(function () {
    breaking_now();
  }, 60000);
  function breaking_now() {
    $.ajax({
      type: 'GET',
      async: true,
      url: "/ajax/checkbreakingnow/q",
      success: function (data) {
        if (data != '') {
          if ($('#newslist').length > 0) {
            $('#newslist').vTicker();
            $('#breaking-ul-update').html(data);
          }
          $('#breakingNow').show();
        } else {
          $('#breakingNow').hide();
        }
      }
    });
    setTimeout(function () {
      breaking_now();
    }, 60000);
  }
  ;</script>
