// whenever you want header bids call this function 
function fetchHeaderBids(batch) {

    if(typeof batch !== "number")
        var apstagSlot = batch.apstagSlot || [];
    else
        var apstagSlot = apstagSlots[batch] || [];   

    // create a requestManager to keep track of bidder state to determine when to send ad server
    // request and what apstagSlots to request from the ad server
    var requestManager = {
        adserverRequestSent: false,     
    };  

    // actually get ads from GAM
    function sendAdserverRequest() {  
        // return early if request already sent
        if (requestManager.adserverRequestSent === true) {
            return;
        }  

        requestManager.adserverRequestSent = true;

        if(typeof apstagSlot !== "undefined")
        {
            apstag.fetchBids({
                slots: apstagSlot,
                timeout: ADS_TIMEOUT
            }, function(bids) {
                googletag.cmd.push(function() {

                    if(typeof batch === "number")
                    {
                        if(void 0 !== gptAdUnit[batch])
                        {
                            apstag.setDisplayBids();
                            googletag.pubads().refresh(gptAdUnit[batch]);                                                                                 
                        }
                    }
                    else
                    {
                        apstag.setDisplayBids();
                        googletag.pubads().refresh([batch.slot]);       
                    }
                });
            });                              
        }
        else
        {
            googletag.cmd.push(function() {
                apstag.setDisplayBids();
                googletag.pubads().refresh(gptAdUnit[batch]);     
            });
        }
    }   

    sendAdserverRequest();

    // set timeout to send request to call sendAdserverRequest() after timeout
    // if all bidders haven't returned before then
    window.setTimeout(function() {
        sendAdserverRequest();
    }, ADS_TIMEOUT); 
}

//fetch first batch
window.setTimeout(function() {
    fetchHeaderBids(0);
}, ADS_TIMEOUT);     
    var adRefresher = function(obj) {
    var isOnFocus = true;

    $(window).on('focus', function(event) {
        event.preventDefault();
        isOnFocus = true;            
        _adHook($(obj.selector).eq(0));

    }).on('blur', function(event) {
        event.preventDefault();
        isOnFocus = false;
        _clearTimer(adTimer);
    });

    function _adHook(object) {

        if(elementScrolled(object) && urlParam('ref') != 'native_scroll' && isOnFocus && !object.hasClass('off')) {
            if(!adTimerCreated) {
                adTimer = createAdTimer();
            }
        } else {
            _clearTimer(adTimer);
        }
    }

    function createAdTimer()
    {
        console.log('ad timer was set for' + obj.selector);
        adTimerCreated = true;     

        return setInterval(function() {
            var slot = obj.unit;
            var willRefresh=void 0!==obj.before&&obj.before()||void 0===obj.before;
            
            if (!slot) {return;}

            if(willRefresh) {
                fetchHeaderBids({
                    "apstagSlot": [obj.apsSlot],
                    "adUnit": [],
                    "slot": obj.unit
                });              
                console.log('ad was refreshed' + obj.selector);
            }
            else {
                console.log('class identifier has detected, end refresh for' + obj.selector);
                $(obj.selector).addClass('off');
                _clearTimer(adTimer);
            }

        }, obj.time);
    }

    function _clearTimer(timer)
    {  
        if(adTimerCreated)
        {
            console.log('ad timer cleared for' + obj.selector);
            clearInterval(timer);
            adTimerCreated = false;
        }
    }

    var adTimerCreated = false;
    var adTimer;

    $(window).bind('scroll', function() {
        if(Object.keys(gptAdUnit).length > 0)
        {
            _adHook($(obj.selector).eq(0));
        }
    }); 
};
