            gtag('event', 'Play', { 'event_category': videotype, 'event_label': vlabel});
            isVideoPlayed = true;
          }
      }

      function pauseEvent(event) {
        var videotype = "wion video";
        gtag('event', 'Pause', { 'event_category': videotype, 'event_label': vlabel});
      }

