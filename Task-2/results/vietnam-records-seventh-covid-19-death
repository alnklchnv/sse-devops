<script type="text/javascript">
    
var pageType = jQuery('meta[name=pageType]').attr("content");

if(pageType == "article"){
    var cX = window.cX = window.cX || {}; cX.callQueue = cX.callQueue || [];
    var cxenseRandom = jQuery('meta[name=cxenseRandom]').attr("content");
    cX.CCE = cX.CCE || {}; cX.CCE.callQueue = cX.CCE.callQueue || [];
    cX.callQueue.push(['setRandomId',cxenseRandom]); // server side generated id
    cX.callQueue.push(['setSiteId', '1145278932220698298']);    
    
     
            cX.callQueue.push(['setCustomParameters', { 'usergroup': 'anonymous' }]);
        
    cX.CCE.callQueue.push(['sendPageViewEvent','smg', '0240b44ae7cc929b3991ca5c126d42a11e5582d4']);
}
else{
    var cX = cX || {}; cX.callQueue = cX.callQueue || [];
    cX.callQueue.push(['setSiteId', '1145278932220698298']);
    cX.callQueue.push(['sendPageViewEvent']);
}
</script>

