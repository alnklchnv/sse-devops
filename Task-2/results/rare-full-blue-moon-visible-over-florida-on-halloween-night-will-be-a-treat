function changeDateFormat(widget){
	var dates_toChange=widget.querySelectorAll('.widget .social-date__text:not(.processed),.widget .social-date-modified__text:not(.processed)');
	if(dates_toChange){
		dates_toChange.forEach(function(date) {
         
          if(!date.innerHTML.trim()==""){
			date.classList.add('processed')
			var date_string=date.innerHTML.toString();
            //date_string=date_string.replace(",","");
			console.log(date_string);
			var slpit_dateText=date_string.split(" ");
			
           
			if(slpit_dateText.length==1){
              let plural=true;
               if(slpit_dateText[0].length==2){
              	if(slpit_dateText[0].charAt(0)=="1"){
                plural=false;
                }
              }
				let modifier=plural ? 'hours ago':'hour ago';
             
				if(slpit_dateText[0].substr(-1)=='m'){
					modifier=plural ? 'minutes ago':'minute ago';
				}
				var new_dateText=slpit_dateText[0].slice(0,-1);
				date.innerHTML=  new_dateText+" "+ modifier;
				date.classList.add("modified");
			}
		}
		});
        
	}
}
  _createElementChangeListener= function (selector, callback) {
    const initializedNodes = new Set([]);

    document.querySelectorAll(selector).forEach((node) => {
        callback(node);
        initializedNodes.add(node);
    });

    const mutationObserver = new MutationObserver(() => {
        const nodes = document.querySelectorAll(selector);
        nodes.forEach((node) => {
            if (!initializedNodes.has(node)) {
                callback(node);
                initializedNodes.add(node);
            }
        });
    });

    mutationObserver.observe(document.body, {
        childList: true,
        subtree: true,
    });
}
  _createElementChangeListener(".widget",changeDateFormat);


</script><!-- End User Code --><span class="js-brick-description hidden" data-name="Newsletter"></span><div class="newsletter-element" id="sNewsletter_0_0_10_0_0_1_0_0_1_0"><div class="" id="sNewsletter_0_0_10_0_0_1_0_0_1_0_0"><div class="text-element" id="sNewsletter_0_0_10_0_0_1_0_0_1_0_0_0">Discover Narcity</div><div class="text-element" id="sNewsletter_0_0_10_0_0_1_0_0_1_0_0_1">Get the latest and greatest stories every day in your inbox.</div><div class="sm-mb-2" id="sNewsletter_0_0_10_0_0_1_0_0_1_0_0_2"><div class="newsletter-element__form get-alerts clearfix"><input type="email" placeholder="Enter email" class="newsletter-element__input" id="newsletter-email"></input><label class="accesibility-hidden" for="newsletter-email">Enter email</label><button class="newsletter-element__submit">Sign me up</button></div></div></div><div class="" id="sNewsletter_0_0_10_0_0_1_0_0_1_0_1"></div></div><span class="js-brick-description hidden" data-name="Firework Carousel Player"></span><div class="mb-2 posts--socialux article-w-keep-reading " id="sSocial_Stream_Element_0_0_13_0_0_1_0_0_1_0_5">




