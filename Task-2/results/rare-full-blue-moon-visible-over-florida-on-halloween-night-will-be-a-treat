  var makeMonitor = (getStatus, cb, delay) => {
    delay = delay || 100;
    var monitor = () => {
        var time = +new Date();
        var status = getStatus();
        if (status) {
          return cb(status);
        } else {
          if (!stopMonitor && +new Date() - time < 30 * 60 * 1000) {
            monitorId = setTimeout(monitor, delay);
          } else {
            cb(false);
          }
        }
      },
      cancel = () => {
        stopMonitor = true;
        clearInterval(monitorId);
      };
    let monitorId,
      stopMonitor = false;
    return {
      start: monitor,
      cancel: cancel,
    };
  };

  var monitorCheckErrorSticky = makeMonitor(
    function () {
      return document.querySelector(
        ".sticky-city-promo button.authentication--submit"
      );
    },
    function (res) {
      trackError(res, "sticky-city-promo", "sticky");
      trackSuccess(res, "sticky-city-promo", "sticky");
    }
  );

  var monitorCheckErrorPost = makeMonitor(
    function () {
      return document.querySelector(
        ".discover-after-post button.authentication--submit"
      );
    },
    function (res) {
      trackError(res, "discover-after-post", "post");
      trackSuccess(res, "discover-after-post", "post");
    }
  );

  monitorCheckErrorSticky.start();
  monitorCheckErrorPost.start();

  function trackError(submit, element, param) {
    let span = document.querySelector(
      `.${element} .authentication--error-feedback`
    );

    submit.addEventListener("click", () => {
      span.addEventListener("DOMSubtreeModified", () => {
        if (span.innerText.includes("email already registered")) {
          let inputEmail = document.querySelector(
            `.${element} input.authentication--input[type='email']`
          );
          inputEmail.style.border = "1px solid red";
          inputEmail.parentNode.insertBefore(
            document.querySelector(`.e-mail-already-exists-error.${param}`),
            inputEmail.nextSibling
          );
          span.classList.add("hidden");
          document
            .querySelector(`.e-mail-already-exists-error.${param}`)
            .classList.remove("hidden");
        }
      });
    });
  }

  function trackSuccess(submit, element, param) {
    let span = document.querySelector(
      `.${element} .authentication--success-feedback`
    );
    console.log(span);
    submit.addEventListener("click", () => {
      span.addEventListener("DOMSubtreeModified", () => {
        let inputEmail = document.querySelector(
          `.${element} input.authentication--input[type='email']`
        );
          setCookie("subscription", inputEmail.value, 1);
      });
    });
  }

  var monitorErrorMessageSticky = makeMonitor(
    function () {
      return document.querySelector(
        ".e-mail-already-exists-error.sticky #send-password-email"
      );
    },
    function (res) {
      buttonSendEmail(res, "sticky-city-promo", "sticky");
    }
  );

  monitorErrorMessageSticky.start();

  var monitorErrorMessagePost = makeMonitor(
    function () {
      return document.querySelector(
        ".e-mail-already-exists-error.post #send-password-email"
      );
    },
    function (res) {
      buttonSendEmail(res, "discover-after-post", "post");
    }
  );

  monitorErrorMessagePost.start();

  function buttonSendEmail(button, elementClass, element) {
    button.addEventListener("click", () => {
      let email = document.querySelector(
        `.${elementClass} input.authentication--input[type='email']`
      ).value;
      sendResetEmail(email, element, elementClass, "resetOld");
    });
  }

  function sendResetEmail(userEmail, element, elementClass, param) {
    console.log(element, elementClass);
    fetch("/core/users/password_reset/", {
      method: "POST",
      mode: "same-origin",
      cache: "no-cache",
      credentials: "same-origin",
      headers: {
        "Content-Type": "application/json",
      },
      body: JSON.stringify({ email: userEmail }),
    })
      .then((res) => {
        if (res.ok) {
          if (param == "resetOld") {
            printMessage(element, elementClass);
          } else {
            printMessageNewSub();
          }
        }
      })
      .catch((res) => false);
  }

  function printMessage(element, elementClass) {
    let checkEmail = document.querySelector(
      `.email-reset-password-sent.${element}`
    );
    let node = document.querySelector(
      `.${elementClass} .signup-form.authentication--form`
    );
    node.parentNode.insertBefore(checkEmail, node.nextSibling);
    checkEmail.classList.remove("hidden");
    node.classList.add("hidden");
  }

  function printMessageNewSub() {
    let checkEmail = document.querySelectorAll(".email-reset-password-sent");
    let node = document.querySelectorAll(".rm-follow-logged-users");

    for (let i = 0; i < checkEmail.length; i++) {
      node[i].parentNode.insertBefore(checkEmail[i], node[i].nextSibling);
      checkEmail[i].classList.remove("hidden");
      node[i].classList.add("hidden");
    }
  }

  window.onload = (event) => {
    console.log("page is fully loaded");
    const email = getCookie("subscription");
    if (email != "") {
      sendResetEmail(email, "newSubs");
      document.cookie =
        "subscription=; expires=Thu, 01 Jan 1970 00:00:00 UTC; path=/;";
    }
  };

  function setCookie(cname, cvalue, exdays) {
    var d = new Date();
    d.setTime(d.getTime() + exdays * 24 * 60 * 60 * 1000);
    var expires = "expires=" + d.toUTCString();
    document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
  }

  function getCookie(cname) {
    var name = cname + "=";
    var decodedCookie = decodeURIComponent(document.cookie);
    var ca = decodedCookie.split(";");
    for (var i = 0; i < ca.length; i++) {
      var c = ca[i];
      while (c.charAt(0) == " ") {
        c = c.substring(1);
      }
      if (c.indexOf(name) == 0) {
        return c.substring(name.length, c.length);
      }
    }
    return "";
  }
