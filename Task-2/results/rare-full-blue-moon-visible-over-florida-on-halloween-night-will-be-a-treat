createAccountEmail.addEventListener("click", () => {
    toggleModal(emailModal);
    toggleModal(createAccModal);
});
  
returnButton.addEventListener("click", () => {
    toggleModal(createAccModal);
    toggleModal(emailModal);
});
  
closeBtnCAModal.addEventListener("click", () => {
    toggleModal(emailModal);
    closeModal();
});
  
signInEmail.addEventListener("click", () => {
    toggleModal(signInEmailModal);
    toggleModal(signInModal);
});
  
returnSIButton.addEventListener("click", () => {
    toggleModal(signInModal);
    toggleModal(signInEmailModal);
});
  
closeBtnSIModal.addEventListener("click", () => {
    toggleModal(signInEmailModal);
    closeModal();
});
  
forgotPasswordButton.addEventListener("click", () => {
	toggleModal(resetPassModal);
  	toggleModal(signInEmailModal);
});
  
closeRPModal.addEventListener("click", () => {
	toggleModal(resetPassModal);
  	closeModal();
});
  
returnToSignIn.addEventListener("click", () => {
	toggleModal(signInEmailModal);
	toggleModal(resetPassModal);
});

document.getElementById("close-pop-up-reset").addEventListener("click", () => {
    toggleModal(document.querySelector(".error-message-pop-up"));
});

function openModal(){
    window.scroll(0, 0);
    toggleModal(footerPage);
    lockBodyScroll();
}

function closeModal(){
    toggleModal(footerPage);
    enableBodyScroll();
}
  
function toggleModal(modal){
	modal.classList.toggle("hidden");
}
  
function lockBodyScroll(){
	document.querySelector('body').style.overflowY = 'hidden';
}
  
function enableBodyScroll(){
	document.querySelector('body').style.overflowY = 'auto';
}


})

