  var monitorDropdownCity = makeMonitor(
    function () {
      return document.querySelector(".authentication--field-Location");
    },
    function (res) {
      res.parentNode.insertBefore(
        document.querySelector(".dropdown-checkbox"),
        res
      );
    }
  );

  monitorDropdownCity.start();

  var monitorFirstOptionCity = makeMonitor(
    function () {
      return document.getElementById("dropdown-city");
    },
    function (res) {
      res.querySelector("#dropdown-firstoption").classList.add("hidden");
      checkboxSubscribe(res);
    }
  );

  function checkboxSubscribe(element) {
    element.addEventListener("change", (event) => {
      city = event.target.value;
      document.querySelector(
        "label.authentication--field-Location input"
      ).value = document.querySelector(
        `#dropdown-city option[value="${city}"]`
      ).innerText;

      if (city == 1) {
        document
          .querySelector(".checkbox-subscribe-city")
          .classList.add("hidden");
        document.querySelector(
          ".checkbox-subscribe-city--input"
        ).checked = false;
      } else {
        document
          .querySelector(".checkbox-subscribe-city")
          .classList.remove("hidden");
      }
    });

    document
      .querySelector(".checkbox-subscribe-city--input")
      .addEventListener("change", (event) => {
        if (event.target.checked) {
          window["rm-after-signup-follow_section"] =
            document.getElementById("dropdown-city").value;
        } else {
          window["rm-after-signup-follow_section"] = undefined;
        }
      });
  }

  monitorFirstOptionCity.start();

  var monitorCheckError = makeMonitor(
    function () {
      return document.querySelector(".authentication--error-feedback");
    },
    function (res) {
      trackError(res);
    }
  );

  monitorCheckError.start();
var email_exist_element=document.querySelector(".e-mail-already-exists-error")
  function trackError(element) {
     console.log("okkkkkk");
    document
      .querySelector("button.authentication--submit")
      .addEventListener("click", () => {
        element.addEventListener("DOMSubtreeModified", () => {
          if (element.innerText.includes("email already registered")) {
            let inputEmail = document.querySelector(
              "input.authentication--input[type='email']"
            );
            inputEmail.style.border = "1px solid red";
            inputEmail.parentNode.insertBefore(
              email_exist_element,
              inputEmail.nextSibling
            );
            element.classList.add("hidden");
            document.querySelector(".e-mail-already-exists-error").classList.remove("hidden");
            document.querySelector(".password-reset-info--sent")? document.querySelector(".password-reset-info--sent").parentNode.removeChild( document.querySelector(".password-reset-info--sent")):"";
		  }
		  else if(element.innerText.includes("Password")){
			element.classList.add("hidden");
            let inputPW = document.querySelector(
              "input.authentication--input[type='password']"
            );
            inputPW.style.border = "1px solid red";
            document.querySelector(".password-requirement").style.color = "red";
		  }
        });
      });
  }

  var monitorErrorMessage = makeMonitor(
    function () {
      return document.querySelector("#send-password-email");
    },
    function (res) {
      res.addEventListener("click", () => {
        sendResetEmail(
          document.querySelector("input.authentication--input[type='email']")
            .value
        );
      });
    }
  );

  monitorErrorMessage.start();

  function sendResetEmail(userEmail) {
    fetch("/core/users/password_reset/", {
      method: "POST",
      mode: "same-origin",
      cache: "no-cache",
      credentials: "same-origin",
      headers: {
        "Content-Type": "application/json",
      },
      body: JSON.stringify({ email: userEmail,post_reset_redirect:"/digital-downtown" }),
    })
      .then((res) => {
        if (res.ok) {
          //document.querySelector(".error-message-pop-up").classList.remove("hidden");
          document.querySelector(".e-mail-already-exists-error").classList.add("hidden");
          document.querySelector(".e-mail-already-exists-error").insertAdjacentHTML('beforebegin','<p class="password-reset-info--sent">Please check your e-mail to set your password.</p>');
          
        }
      })
      .catch((res) => false);
  }

  var monitorPlaceholder = makeMonitor(
    function () {
      return document.querySelector(".authentication--field-FirstName input");
    },
    function (res) {
      res.placeholder = "First name";
      res.value = "";
    }
  );

  monitorPlaceholder.start();
  
var monitorResetPW = makeMonitor(
    function () {
      return document.querySelector(".modal.reset-password .authentication--form.reset-form");
    },
    function (res) {
      res.querySelector(".authentication--input").setAttribute("placeholder","Enter your email");
      window.asdf=  res.querySelector(".authentication--submit");
      asdf.innerText="Send Instructions";
      //alert( res.querySelector(".authentication--submit"))
     
    }
  );
  monitorResetPW.start();
  
  window.IcreateElementChangeListener= function (selector, callback) {
    const initializedNodes = new Set([]);

    document.querySelectorAll(selector).forEach((node) => {
        callback(node);
        initializedNodes.add(node);
    });

    const mutationObserver = new MutationObserver(() => {
        const nodes = document.querySelectorAll(selector);
        nodes.forEach((node) => {
            if (!initializedNodes.has(node)) {
                callback(node);
                initializedNodes.add(node);
            }
        });
    });

    mutationObserver.observe(document.body, {
        childList: true,
        subtree: true,
    });
}
  IcreateElementChangeListener('.authentication--field-last_name input',function(item){
    item.value=".";
  })
