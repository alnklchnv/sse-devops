placement.insertAdjacentHTML("beforebegin", '<div class="fireworkcarousel"><div class="fireworkheading">Trending Now &nbsp;&#128064;</div><fw-embed-feed channel="narcity_canada" share_template="http://www.narcity.com" playlist="gM0reo" mode="row" open_in="default" max_videos="0" placement="middle" branding="false" player_placement="top-right" vast_attrs="canada&cust_params='+encodeURIComponent(params)+'"></fw-embed-feed></div>')
    

});

