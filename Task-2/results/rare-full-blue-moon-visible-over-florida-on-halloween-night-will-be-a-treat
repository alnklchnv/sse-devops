const floatingObserverConfig = {
  root: null,
  rootMargin: '600px',
  threshold: 1
};

var floatingObserver = new IntersectionObserver(entries => {

  entries.forEach(entry => {
    if (entry.isIntersecting) {
      var entryVideo = entry.target.querySelector('.widget__video');

      isLeaving = true;
      if (isLeaving) {

        if (entryVideo.querySelector('.article-body [type="video-content"]')) {
          //if(document.querySelector(".close-button")){
			document.querySelector(".close-button").classList.add("close-button-hide");
          //}
          entryVideo.querySelector('.article-body [type="video-content"]').classList.remove('rumble-sticky-video')
          if(entryVideo.querySelector('.widget__floating-headlines')){
          entryVideo.querySelector('.widget__floating-headlines').classList.remove('headline-sticky');
        	}
          if(entryVideo.querySelector('.widget__floating-info')){
          entryVideo.querySelector('.widget__floating-info').classList.remove('info-sticky');
          }
          entryVideo.classList.remove("js-hidden-panel")
          if( entryVideo.querySelector('video')){
          		entryVideo.querySelector('video').style.height = "100%";
             if(window.innerWidth > 450){
              entryVideo.querySelector('video').parentElement.style.maxHeight = "370px";
            }
            else{
              entryVideo.querySelector('video').parentElement.style.maxHeight = "211px";
            }
            }
          entryVideo.classList.remove('rumble-sticky--show');
        }
        isLeaving = false;
      }

    } else {
      var entryVideo = entry.target.querySelector('.widget__video');


      if (entryVideo.querySelector('.article-body [type="video-content"]')) {
        entryVideo.classList.add("js-hidden-panel");
        if(document.querySelector(".close-button")){
        	document.querySelector(".close-button").classList.remove("close-button-hide");
        }
        entryVideo.querySelector('.article-body [type="video-content"]').classList.add('rumble-sticky-video')

		if(entryVideo.querySelector('.widget__floating-headlines')){
        entryVideo.querySelector('.widget__floating-headlines').classList.add('headline-sticky');
        }
         if(entryVideo.querySelector('.widget__floating-info')){
        entryVideo.querySelector('.widget__floating-info').classList.add('info-sticky');
         }
        //entry.target.style.height = entryVideo.querySelector('iframe').offsetHeight + "px";
        entry.target.style.height = entry.target.offsetHeight + "px";
        if( entryVideo.querySelector('video')){
        		entryVideo.querySelector('video').style.height = "170px";
              entryVideo.querySelector('video').parentElement.style.maxHeight = "170px";
            }
        entryVideo.classList.add('rumble-sticky--show')
      }
      isLeaving = true;
    }


  });
}, floatingObserverConfig);








