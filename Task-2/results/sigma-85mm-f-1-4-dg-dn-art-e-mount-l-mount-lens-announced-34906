	$('.mobile-more').click(function(){
		dropdown = '.'+this.id.replace('mobile-','');
		$(dropdown).slideToggle();
		val = $(this).html();
		if(val == '+'){ $(this).html('-') } else { $(this).html('+')}
	});	
  
  </script>
  
