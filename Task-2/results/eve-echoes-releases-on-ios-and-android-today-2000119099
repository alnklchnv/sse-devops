<script>
    $(function() {
      var gids = [];
      $('a[data-giveaway]').each(function(){
        if($.inArray($(this).data('giveaway'), gids) === -1) gids.push($(this).data('giveaway'));
      });
      //console.log(gids.join(','));

      if(gids.length > 0){
        $.ajax({
          url: "/ajax/tools.cfc?method=key_counts&gids="+gids.join(','),
          dataType: "json",
          success: function (response){
            //console.log(response);
            $.each(response, function(key,val) {
              $('a[data-giveaway="'+key+'"]').html('<i class="far fa-key-skeleton mr-2"></i>'+val);
              $('span[data-giveaway="'+key+'"]').html(val);
            });
          },
          error: function (jqXHR, textStatus, errorThrown) {
            console.log('Key Counts Error: Status='+textStatus+', Error:'+errorThrown);
          }
        });
      }
    });
  </script>
</body>
</html>
<script>
			// Open External Links in a new tab / window
			function ExternalLinks(domain){
				var hostname = domain.replace("www.", "").toLowerCase();
				var fhostname = 'forums.mmorpg.com';
				var i = document.links.length;
				while ( --i >= 0 ) {
					var thisLink = document.links[i];
					var thisHref = thisLink.href.toLowerCase();
					if ( (thisHref.indexOf("http://") != -1 || thisHref.indexOf("https://") != -1) && thisHref.indexOf(hostname) == -1 && thisHref.indexOf(fhostname) == -1 ){
						thisLink.target = '_blank';
						//thisLink.rel = 'nofollow';
					}
				}
			}
		</script>
<script>		
		function onLoads(){
		ExternalLinks("www.mmorpg.com"); 
		}

		document.onload = onLoads();
	</script>
