 <!-- end of bottom/all.esi --></div><script>consoleLog('inside bsCoralComment');
//- Call Gigya Login Logout Hook on page load
setTimeout(gigyaOnLoginLogoutHook, 500);

window.isCoralInstanceCreated = false;
$('#comments a').bind('click', function (e) {
	$('#comments-container').toggleClass('hide');
	$('#comments-container').toggleClass('show');
	if ($('#comments-container').hasClass('show')) {
		$('#comments a').text('Hide comments');
	} else {
		$('#comments a').text('Leave a comment');
	}

	//- Create Coral Instance only once
	if(!window.isCoralInstanceCreated) {
		consoleLog('createCoralInstance called.');
		createCoralInstance();
		window.isCoralInstanceCreated = true;
	}
	e.preventDefault();
});

function afterGigyaLogin(user){
	consoleLog('afterGigyaLogin');
	// Login the user in Coral with the correct token
	if(window.isCoralInstanceCreated) {
		createJWTToken(user);
	}
}

function afterGigyaLogout(){
	// Logout the user from Coral as well
	consoleLog('afterGigyaLogout');
	window.coral_embed.logout();
}

function afterGigyaUpdated(){
	// Logout the user from Coral as well
	consoleLog('afterGigyaUpdated');
	//window.coral_embed.logout();
	localStorage.removeItem('gigyaCoralToken');
	if(window.gigya){
		window.gigya.accounts.getAccountInfo({'callback': function(user){
			if(user.isRegistered){
				createJWTToken(user);
			}
		}});
	}
}

function gigyaOnLoginLogoutHook() {
	consoleLog('inside gigyaOnLoginLogoutHook');
	if (window.news_gigya) {
		// Wait for news-gigya-ui-update-on-login to fire (only occurs when gigya updates the page with user details)
		window.news_gigya.on('news-gigya-ui-update-on-login', function (e, user) {
			if (user.isRegistered) {
				consoleLog('inside news-gigya-ui-update-on-login');
				afterGigyaLogin(user);
			}
		});

		// On logout call the afterGigyaLogout function
		window.news_gigya.on('news-gigya-ui-update-on-logout', function (e, user) {
			consoleLog('inside news-gigya-ui-update-on-logout');
			afterGigyaLogout();
		});

		// Wait for news-gigya-ui-update-on-login to fire (only occurs when gigya updates the page with user details)
		window.news_gigya.on('screenset.submitted', function (e, screen) {
			consoleLog('inside screensset.submitted');
			if (screen.screen === 'update-profile') {
				afterGigyaUpdated();
			}
		});

	} else {
		setTimeout(gigyaOnLoginLogoutHook, 500);
	}
}

// New Coral Comment Code
function createCoralInstance() {
	consoleLog('inside createCoralInstance');
	consoleLog('canonical: ' + 'https://www.bodyandsoul.com.au/health/health-news/children-under-5-can-carry-higher-levels-of-coronavirus-than-adults-study-finds/news-story/ab52a5dcf8b40813a2ae764da0ed01bc');
	consoleLog('storyURL: ' + 'https://www.bodyandsoul.com.au/health/health-news/children-under-5-can-carry-higher-levels-of-coronavirus-than-adults-study-finds/news-story/ab52a5dcf8b40813a2ae764da0ed01bc');
	consoleLog('storyID: ' + 'ab52a5dcf8b40813a2ae764da0ed01bc');
	// Coral
	var d = document, s = d.createElement('script');
	var url = 'newscorpaustralia.coral.coralproject.net';
	s.src = '//' + url + '/assets/js/embed.js';
	s.async = false;
	s.defer = true;
	s.onload = function() {
		window.coral_embed = Coral.createStreamEmbed({
			id: "comments-container",
			bodyClassName: "nlm_bodyandsoul",
			autoRender: true,
			rootURL: '//' + url,
			storyURL: 'https://www.bodyandsoul.com.au/health/health-news/children-under-5-can-carry-higher-levels-of-coronavirus-than-adults-study-finds/news-story/ab52a5dcf8b40813a2ae764da0ed01bc',
			storyID: 'ab52a5dcf8b40813a2ae764da0ed01bc',
			events: function(events) {
				events.on('loginPrompt', function(){
					// Call gigya login overlay from coral UI
					window.news_gigya.showLogin(new CustomEvent('click'));
					return false;
				});
			},
		});
		if(window.gigya){
			window.gigya.accounts.getAccountInfo({'callback': function(user){
				if(user.isRegistered){
					createJWTToken(user);
				}
			}});
		}
	};
	(d.head || d.body).appendChild(s);
};

function createJWTToken(user) {
	consoleLog('inside createJWTToken');
	var localToken = getWithExpiry('gigyaCoralToken');
	consoleLog('localToken: ');
	consoleLog(localToken);
	if(localToken === null){
		var xhr = new XMLHttpRequest();
		var params = [
			'ApiKey=' + encodeURIComponent(window.gigya.apiKey),
			'UID=' + encodeURIComponent(user.UID),
			'UIDSignature=' + encodeURIComponent(user.UIDSignature),
			'signatureTimestamp=' + encodeURIComponent(user.signatureTimestamp),
		];
		var req = 'https://commerceapi.news.com.au/identity-gigya/get-coral-token' + '?' + params.join('&');
		xhr.open("GET", req, true);
		xhr.onreadystatechange = function () {
			if (xhr.readyState === XMLHttpRequest.DONE) {
				if (xhr.status === 200) {
					json = JSON.parse(xhr.responseText);
					consoleLog('apiResponse: ');
					consoleLog(json);
					json = json.result;
					setWithExpiry('gigyaCoralToken', json['token'], 3600000);	// store the token in localStorage for 3600000ms = 1hour
					window.coral_embed.login(json['token']);
				}
			}

		};
		xhr.send();
	}else{
		consoleLog('local token exists');
		window.coral_embed.login(localToken);
	}
}

function setWithExpiry(key, value, ttl) {
	var today = new Date();
	var itemObj = {
		value: value, 					// `itemObj` is an object which contains the original value
		expiry: today.getTime() + ttl,  // as well as the time when it's supposed to expire
	};
	localStorage.setItem(key, JSON.stringify(itemObj));
}

function getWithExpiry(key) {
	var itemObj = localStorage.getItem(key);
	if (!itemObj) {
		return null;   // if the key doesn't exist, return null
	}
	var item = JSON.parse(itemObj);
	var today = new Date();
	if (today.getTime() > item.expiry) {  // compare the expiry time of the item with the current time
		localStorage.removeItem(key);     // If the item is expired, delete the item from storage and return null
		return null;
	}
	return item.value;
}

function consoleLog(obj) {
	if ('prod' !== 'prod' || false) console.log(obj);
}
