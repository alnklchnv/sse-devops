<div id="block-block-56" class="block block-block col-lg-12">
   <div class="block-inner">
              <!--<h2><span></span></h2>-->
        
    <div class="content">
      <script>
	/*jQuery(document).ready(function($) {
		//scroller for table
		$("table").each(function(){
			if($(this).width() > $('.node-article .field-name-body .field-items').width())
			{
					
				if($(this).height() < 500)
				{
					$(this).wrap("<div class='scroll' style='width:100%; overflow: scroll auto;'></div>");	
				}
				else{
					$(this).wrap("<div class='scroll' style='width:100%; height:500px; overflow: scroll;'></div>");	
				}
			}
		});
	});*/
	var art_class = document.getElementsByClassName('node-article')[0]; 
	var art_table = art_class.getElementsByTagName('table');
	
	for(i=0; i<art_table.length; i++)
	{
		if(art_table[i].offsetWidth > art_class.offsetWidth)
		{
			if(art_table[i].offsetHeight < 500)
			{
				var wrapper = document.createElement('div');
				wrapper.setAttribute('class', 'scroll');
				wrapper.setAttribute('style', 'width:100%; overflow: scroll auto;');
				art_table[i].parentNode.insertBefore(wrapper, art_table[i]);
				wrapper.appendChild(art_table[i]);
			}
			else{
				var wrapper = document.createElement('div');
				wrapper.setAttribute('class', 'scroll');
				wrapper.setAttribute('style', 'width:100%; height:500px; overflow: scroll;');
				art_table[i].parentNode.insertBefore(wrapper, art_table[i]);
				wrapper.appendChild(art_table[i]);
			}
		}
	}
</script>	
    </div>
