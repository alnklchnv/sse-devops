<script>
jQuery(document).ready(function($){
	$( ".search-icon" ).click( function()
   {	
      var term ='';
      term = $("#homesearchText").val();
      window.location = '/search?term=' + term;
   });
	$( "#homesearchText" ).keypress( function(e)
   {
	   
	   console.log('t');
	   if(e.which == 13){
		   e.preventDefault();
      var term ='';
      term = $("#homesearchText").val();
	   window.location = '/search?term=' + term;
	   }
   });
	
});
