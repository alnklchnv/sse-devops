}
function getCMPId(encodedTCString) {
var bitField = base64decode(encodedTCString);
var bitsBeforeCmpId = 78;
var cmpIdBits = 12;
var bits = bitField.substr(bitsBeforeCmpId, cmpIdBits);
return parseInt(bits, 2);
}
