    <script>
        document.addEventListener("DOMContentLoaded", function () {
            var lazyAds = adLazyloaded;
            var active = false;

            const lazyLoad = function () {
                if (active === false) {
                    active = true;

                    setTimeout(function () {
                        lazyAds.forEach(function (lazyAd) {
                            var adSlot = document.getElementById(lazyAd.getSlotElementId());
                            if (adSlot && (adSlot.parentElement.getBoundingClientRect().top <= (window.innerHeight + 300) && adSlot.parentElement.getBoundingClientRect().bottom >= 0) && getComputedStyle(adSlot.parentElement).display !== "none") {
                                console.log(lazyAd.getSlotElementId());
                                googletag.cmd.push(function () {
                                    if (typeof OptanonActiveGroups !== 'undefined' && OptanonActiveGroups.match(/,C0004,/)) {
                                        googletag.pubads().refresh([lazyAd]);
                                    }
                                });
                                lazyAds = lazyAds.filter(function (ad) {
                                    return ad !== lazyAd;
                                });

                                if (lazyAds.length === 0) {
                                    document.removeEventListener("scroll", lazyLoad);
                                    window.removeEventListener("resize", lazyLoad);
                                    window.removeEventListener("orientationchange", lazyLoad);
                                }
                            }
                        });
                        active = false;
                    }, 200);
                }
            };
            document.addEventListener("scroll", lazyLoad);
            window.addEventListener("resize", lazyLoad);
            window.addEventListener("orientationchange", lazyLoad);
        });
    </script>
