jq('#payfail_modal .pay_btn').click(function f(){
    window.open(window.location.origin + '/myaccount/','_blank')
    jq.cookie('IT_pw_hidepaymodaluntil', Date.now() + timeToHideModal)
    jq('#payfail_modal').toggleClass('hidden',1);
})

jq('#payfail_modal .dismiss_btn').click(function f(){
    jq.cookie('IT_pw_hidepaymodaluntil', Date.now() + timeToHideModal);
