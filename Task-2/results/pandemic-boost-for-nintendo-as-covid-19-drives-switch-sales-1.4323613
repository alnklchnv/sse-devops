<script>
    // START Custom Prompt 2 window workflow

    // number of clicks for first time users to trigger Custom Prompt
    var firstAttemptClicks = 3;
    // number of clicks to re-trigger Custom Prompt for users who denied/ignored Custom Prompt
    var reAttemptClicks = 50;
    // read data from XP on Application -> Local Storage -> qa.irishtimes.com -> xtremepush.data
    var xp = JSON.parse(localStorage.getItem('xtremepush.data'));

    var postLoad = document.getElementById('irishtimes-postload');
    var countdownTimer = document.getElementById("countdowntimer");

    // To check user's permission
    xtremepush('ready', function () {
        var permission = xtremepush('push', 'permission');
        if(permission == "default"){
            //show custom prompt
            if (xp.page_views == firstAttemptClicks) {
                postLoad.classList.remove('hideThis');
            } else if (xp.page_views >= (firstAttemptClicks + reAttemptClicks)) {
                // re-trigger Custom Prompt after first attempt of clicks
                var page_views = xp.page_views;
                var clicks = page_views - firstAttemptClicks;
                if (clicks % reAttemptClicks == 0) {
                    postLoad.classList.remove('hideThis');
                }
            }
        }
    });

    var timeleft = 5;
    // I AM INTERESTED window
    function wpnAllowed() {
        document.getElementById("wpn-body_allowed").style.display = "block";
        document.getElementById("wpn-body").style.display = "none";
        // Timer starts
        countdownTimer.innerHTML = timeleft;
        countDown(timeleft, true);
    }

    // NOT NOW window
    function wpnDenied() {
        document.getElementById("wpn-body_denied").style.display = "block";
        document.getElementById("wpn-body").style.display = "none";
        // Timer starts
        countdownTimer.innerHTML = timeleft;
        countDown(timeleft, false);
    }

    /**
     * Timer for popping up the System Prompt and closing Custom Prompt
     * @param timeleft set seconds from CMS under Et Cetera XP Config
     * @param systemPrompt boolean (true value passed from I AM INTERESTED window)
     */
    function countDown(timeleft, systemPrompt) {
        countdownTimer.classList.add('bg-timer');
        var downloadTimer = setInterval(function(){
            timeleft--;
            countdownTimer.innerHTML = timeleft;
            if(timeleft == 0) {
                countdownTimer.classList.add('bg-timer');
                clearInterval(downloadTimer);
                if (systemPrompt === true) {
                    // Selectively enable auto prompt
                    xtremepush('push', 'prompt');
                }
                // Hides the Custom Prompt
                postLoad.classList.add('hideThis');
            }
        },1000);
    }
    // END Custom Prompt 2 window workflow
