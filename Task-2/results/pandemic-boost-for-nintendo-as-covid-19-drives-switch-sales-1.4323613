    // Accordion functions
    jq(function () {
        var Accordion = function (el, multiple) {
            this.el = el || {};
            this.multiple = multiple || false;

            // Variables privadas
            var links = this.el.find('.link');
            // Evento
            links.on('click', {
                el: this.el,
                multiple: this.multiple
            }, this.dropdown)
        }

        Accordion.prototype.dropdown = function (e) {
            var jqel = e.data.el;
            jqthis = jq(this),
                jqnext = jqthis.next();

            jqnext.slideToggle();
            jqthis.parent().toggleClass('open');

            if (!e.data.multiple) {
                jqel.find('.submenu').not(jqnext).slideUp().parent().removeClass('open');
            };
        }

        var accordion = new Accordion(jq('#linksAccordion'), false);
    });
</script>

