	<script>
	var use_autogrow = true;

	function thumbs_up(cid,subtype){
		$.post('/ajax/components/ajax',{'_comp':'/components/generic_ratings/rate',rating:1,type:subtype,target_id:cid});
		var new_val = $('#rating_count_' + cid).length > 0 ? parseInt($('#rating_count_' + cid).text()) : 0;
		new_val++;
		if(new_val > 1){
			$('#did_rate_'+cid).html('<span>' + new_val + '</span>' + " people liked this");
		} else {
			$('#did_rate_'+cid).html('<span>' + new_val + '</span>' + " person liked this");
		}
	}

	function show_hidden_autogrow(form){
		$('#' + form + ' textarea').autogrow();
	}

	function createNickname(event){
		var sel = event.target;
		if(sel.options[sel.selectedIndex].value == -1){
			$('#createNickname').show();
			$('html body').animate({
				scrollTop: $('#createNickname').offset().top
			});
			$('#nickname').focus();

		}

	}



	function createNicknameSelect(){
		$.post('/ajax/components/ajax',{'_comp':'/components/forums/nicknames/data:form_data',news:1},function(data){

			var nickname = data;
			$('.nickname_select').each(function(i,seldiv){
				seldiv = $(seldiv);
				if(/_[1-9]/.test(seldiv.attr('id'))){
					seldiv.html("Post Reply As: ");
				}else{
					seldiv.html("Post Comment As: ");
				}

				var sel = $('<select name="use_nickname" id="comment_form_' + seldiv.attr('id') + '" class="form-control">');
				sel.change(createNickname);
				seldiv.append(sel);
				data.forEach(function(row){
					sel.append($("<option value='" + row.id + "'>" + row.value + "</option>"));
				});

			});

		},'json');
	}

	function check_discussion_form(cid){
		$('#comment_reply_' + cid).attr('disabled',true);
		if($('#comment_form_nickname_select_' + cid).length > 0 && $('#comment_form_nickname_select_' + cid).val() == -1){
			alert("please create a nickname or post as yourself");
			$('comment_reply_' + cid).attr('disabled',false);
			return;
		}

		if($('#comment_form_text_' +cid).val().length > 0 && $('#comment_form_text_'+ cid).val() != 'Enter your reply here...'){			
			$('#comment_form_' +cid).submit();
		}else{
			alert("Please enter in a comment");
			$('#comment_reply_' + cid).attr('disabled',false);
		}

	}

	function forum_subscribe(pid,did,sid,serviceid){
		$('#subscribe_button').load('/ajax/components/ajax?_comp=%2Fcomponents%2Fdiscussions/subscribe&sid=' + sid + '&pid='+pid + "&did=" + did + "&serviceid=" + serviceid);
	}


	function rating_details(rate_id,uid){
		$('#details_' + rate_id).hide();
		$('#rating_details_' + rate_id).show();
		$('#rating_details_' + rate_id).load('/ajax/components/ajax?_comp=%2Fcomponents%2Fdiscussions/show_ratings:details&rate_id='+ rate_id + '&uid=' + uid + '&jquery=1&new=');
	}

	function toggle_tab(id){
		$('#auctionTabs').children().each(function(tab){
			if(id == tab.id){
				$(id).addClass('myButtonMenuPressed');
				$(id + '_tab').show();
			}else{
				$(tab.id).removeClass('myButtonMenuPressed');
				$(tab.id + '_tab').hide();
			}	
		});
	}

	domReadyQueue.push(function($){
		if(use_autogrow){
