                icon: "https://hothardware.com/favicon.ico",
                url: "https://hothardware.com/account/signin?dlg=true",
                logout: "https://hothardware.com/account/signout",
                width: "634",
                height: "600"
            };
        }


        /* * * CONFIGURATION VARIABLES: THIS CODE IS ONLY AN EXAMPLE * * */
        var disqus_shortname = 'hothardware';
        var disqus_developer = 1;
        var disqus_identifier = '1_52530';
        var url = 'https://hothardware.com/news/more-geforce-rtx-3090-details-allegedly-leaked-including-memory-configs-and-clock-speeds';

        function disqusDefer() {
            if (window.jQuery) {
                if (!jQuery("#disqus_thread").length) {
                        jQuery('<div id="disqus_thread"></div>').insertAfter(jQuery('#initdisqus'));
                    }
                    loadDisqus(jQuery('#initdisqus'), disqus_identifier, url);


            }
            else {
                setTimeout(function () { disqusDefer(); }, 50);
            }
        }



        function loadDisqus(source, identifier, url) {

            if (jQuery("#disqus_thread").length) {
                jQuery("#disqus_thread").remove();
            }
            jQuery('<div id="disqus_thread"></div>').insertAfter(source);

            if (window.DISQUS) {

                DISQUS.reset({
                    reload: true,
                    config: function () {
                        this.page.identifier = identifier;
                        this.page.url = url;
                    }
                });

            } else {

                //insert a wrapper in HTML after the relevant "show comments" link

                disqus_identifier = identifier; //set the identifier argument
                disqus_url = url; //set the permalink argument

                //append the Disqus embed script to HTML
                var dsq = document.createElement('script'); dsq.type = 'text/javascript'; dsq.async = true;
                dsq.src = 'https://' + disqus_shortname + '.disqus.com/embed.js';
                jQuery('head').append(dsq);

            }

            jQuery('.show-disqus').show();
            source.hide();
        };

        function disqusEvent()
        {
            idleTime = 0;
    }

    var disqusLoaded = false;
    $(window).scroll(function () {
        if (disqusLoaded) {
            return;
        }

        var top = window.innerHeight + window.scrollY;
        var distance = $('.disqus-area').offset().top - top;

        // 30 is an arbitrary padding choice, 
        // if you want a precise check then use distance===0
        if (distance < 200 && !disqusLoaded) {
            disqusLoaded = true;
            disqusDefer();
        }
    });
</script>

