    ///////////////////// text to  speech functions//////
    function speak(obj) {
        $(obj).articulate('speak');
    };

    function pause() {
        $().articulate('pause');
    };

    function resume() {
        $().articulate('resume');
    };

    function stop() {
        $().articulate('stop');
    };

    ////////////////////////////////////////////////////////////




    function LikeNews(obj, isLike) {
        if ($(obj).hasClass("disabled-link") == false) {
            $(obj).addClass("disabled-link");
            if (isLike) {
                $('.dislike-news a').addClass("disabled-link");
            } else {
                $('.like-news a').addClass("disabled-link");
            }
            $(obj).css("color", "#0056b3");

            AddLikeCount(124543, isLike);
        }
    }


    function AddRelatedNews() {
        var news = $($('.news-wrap-block')[0]).next().text().trim();
