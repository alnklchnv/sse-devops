        var isbold = $('.story-desc p:not(.p-ignore):lt(2)').find("strong").length;
        var src = $('#hdnSrc').val();
        if (isbold == 0 && (src == "" || src.toLowerCase() != "print")) {
            $(".p-ignore").show();
